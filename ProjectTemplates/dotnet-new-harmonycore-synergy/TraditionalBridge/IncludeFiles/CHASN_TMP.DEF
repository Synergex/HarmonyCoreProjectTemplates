; CHASN_TMP.DEF
;
;	Temporary ASN File to containing the ASN Information to be Sent to
;	Chrysler.
;
;	File Organization: Sequential		Record Size: 45
;
;-------------------------------------------------------------------------------
;
RECORD CHASN_TMP		;  ** HEADER SHIPMENT RECORD (HSR) **
	CTH_SUPPLIER	,A5		; Supplier Id Code.
	CTH_SHIP_DATE	,D4		; Ship Date, Format: MMDD.
	CTH_CONV_CODE	,A1		; Conveyance Code.
	CTH_CARRIER	,A7		; Carrier.
	CTH_CONV_NO	,D6		; Conveyance Number.
	CTH_BILL_LADING	,D8		; Bill of Lading (use Shipper #).
	CTH_CONTAINER	,A1		; Container Code.
	CTH_CONT_QTY	,D3		; Number of Containers.
	CTH_WEIGHT	,D5		; Gross Weight.
	CTH_TRANS_CODE	,D2		; Transaction Code
					; ----------------
					; 29 = Original Transmission.
					; 85 = Cancelled ASN.
	CTH_FILLER	,A2		; Filler.
	CTH_RECORD_ID	,D1		; Record Id (Always 1)

RECORD ,X			;  ** DETAIL SHIPMENT RECORD (DSR) **
	CTD_CUST_PART	,A9		; Chrysler Part Number.
	CTD_PACK_SLIP	,D8		; Shipper Number/Packing Slip.
	CTD_DEST_CODE	,A4		; Destination Code.
	CTD_SHIP_QTY	,D7		; Quantity Shipped.
	CTD_YTD_CUM	,D8		; YTD Cum Shipped.
	CTD_FILLER	,A8		; Filler
	CTD_RECORD_ID	,D1		; Record Id (Always 2)
;:
