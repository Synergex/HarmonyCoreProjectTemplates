; RT850_DTL.DEF
;
;	Retail X12 Purchase Order (850) Detail File.
;
;	Date		Analyst		Modification
;	-----------	---------------	----------------------------------------
;	11-Jan-1993	Bill Schmelter	Increased record size from 320 to 480
;	05-Nov-1993	Bill Schmelter	Added RTD_COLOR,CUST_ITM_QL
;	14-Dec-1993	Bill Schmelter	Added RTD_AMOUNT,RTD_AMOUNT_QL
;	14-Dec-1993	Bill Schmelter	Increased record size from 480 to 640
;-------------------------------------------------------------------------------
; File Organization: Sequential | Record Size: 640	|  Positions
;		    		| Variable Length	|
;-------------------------------------------------------------------------------

RECORD RT850_DTL
		;----- Sort Key Information -------------
	RTD_SUPP_ID	,A15	; Vendor ID Number	|   1 thru 15
	RTD_PO_NO	,A22	; Purchase order number |  16 thru 37
	RTD_PO_DATE	,D6	; Purchase order date	|  38 thru 43
	RTD_PO_TYPE	,A2	; Purchase order type	|  44 thru 45
				;			|
	RTD_PO_LINE	,D6	; PO Line Number	|  46 thru 51
	RTD_CUST_ITEM	,A22	; Customer Partno	|  52 thru 73
				;			|
		;------Additional Part Information ------
	RTD_UPC		,A12	; UPC Code		|  74 thru 85
	RTD_VND_ITEM	,A30	; Vndrs Item/or PartNo	|  86 thru 115
	RTD_CAT_ITEM	,A30	; Vndrs/Byrs Catalog No	| 116 thru 145
				;			|
	RTD_UOFM	,A2	; Unit of Measure	| 146 thru 147
	RTD_UNIT_COST	,D12	; Unit Price/Cus Cst 8v4| 148 thru 159
	RTD_UP_BASIS	,A2	; Unit Price Basis T#639| 160 thru 161
	RTD_ORD_QTY	,D13	; Tot Qty Ordered    9v4| 162 thru 174
	RTD_C_RETAIL	,D14	; Cust Retail Price 12v2| 175 thru 188
				;			|
		;----- Packing Information -------------|
	RTD_PK		,D6	; Pack Qty(#ofPack)	| 189 thru 194
	RTD_PK_SIZE	,D8	; Qty Per Pack 		| 195 thru 202
	RTD_PK_UOFM	,A2	; PO4 Unit of Measure	| 203 thru 204
	RTD_PK_WGT_TP	,A1	; Pack Weight Type	| 205 thru 205
				; G=Gross,N=Net  Tbl#187| 
	RTD_PK_WGT	,D8	; Pack Wgt 	6v2	| 206 thru 213
	RTD_PK_UOFM2	,A2	; Pack Wgt? UofM	| 214 thru 215
	RTD_PK_DESCR	,A80	; Pack Description/Text	| 216 thru 295
				;			|
		;-- P/O Change Information -------------|
	RTD_CHANGE_TYPE	,A2	; Line Item Change Type	| 296 thru 297
				; --------------------- |
				; "AI" = Add Item	|
				; "DI" = Delete Item	|
				; "PC" = Price Change	|
				; "QI" = Qty Increase   |
				; "QD" = Qty Decrease   |
	RTD_SIZE_CODE	,A8	; Buyer's Size Code	| 298 thru 305
	RTD_RECV_QTY	,D13	; Qty Left To Receive	| 306 thru 318
				;			|
		;-- PO3 - Additional Item Detail -------|
		;-- Variations in Price Structue -------|
	RTD_PO3_CNG_REAS,A2	; Change Reason Cd T#371| 319 thru 320
	RTD_PO3_DATE	,D6	; Po3 Date		| 321 thru 326
	RTD_PO3_PRICE_QL,A3	; PO3 Price Qual Cd #236| 327 thru 329
	RTD_PO3_UNIT_PRI,D12	; Unit Price 8v4        | 330 thru 341
	RTD_PO3_PRI_BASIS,A2	; Basis Unit Price #639 | 342 thru 343
	RTD_PO3_QTY	,D13	; PO3 Quantity 9v4	| 344 thru 356
	RTD_PO3_UOFM	,A2	; Unit of Meas Code #355| 357 thru 358
	RTD_PO3_DESCR	,A80	; Description		| 359 thru 438
				;
		;---------------------------------------|
	RTD_COLOR	,A10	; Product id Color	| 439 thru 448
	RTD_CUST_ITM_QL	,A2	; Customer Partno Qual	| 449 thru 450
	RTD_AMOUNT_QL	,A2	; 3 Amount qualifiers	| 451 thru 452
	RTD_AMOUNT	,D10	; 3 Amounts 8v2		| 453 thru 462
			,A178	; Filler		| 472 thru 640
				; 			|
RECORD, X
	RTD_KEY		,A73	; Sort/compare key


.IFDEF XX850_DTL_WORK
RECORD RT850_DTL_WORK
