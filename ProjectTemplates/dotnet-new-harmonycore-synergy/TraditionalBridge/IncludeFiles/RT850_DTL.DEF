;	exception of the additional fields in this record of RTD_COMP_W
;	RTD_SEQN_W.
;
;	STRUCTURE:	Sequential
;	LENGTH:		653
;	SORT KEY:	RTD_COMP_W and RTD_SEQN_W
;
	RTD_COMP_W		,A8	; Company Code
	RTD_SEQN_W		,D5	; Sequence Number
		;----- Sort Key Information -------------
	RTD_SUPP_ID_W		,A15	; Vendor ID Number
	RTD_PO_NO_W		,A22	; Purchase order number
	RTD_PO_DATE_W		,D6	; Purchase order date
	RTD_PO_TYPE_W		,A2	; Purchase order type
					;
	RTD_PO_LINE_W		,D6	; PO Line Number
	RTD_CUST_ITEM_W		,A22	; Customer Partno
					;
		;------Additional Part Information ------
	RTD_UPC_W		,A12	; UPC Code
	RTD_VND_ITEM_W		,A30	; Vndrs Item/or PartNo
	RTD_CAT_ITEM_W		,A30	; Vndrs/Byrs Catalog No
					;
	RTD_UOFM_W		,A2	; Unit of Measure
	RTD_UNIT_COST_W		,D12	; Unit Price/Cus Cst 8v4
	RTD_UP_BASIS_W		,A2	; Unit Price Basis T#639
	RTD_ORD_QTY_W		,D13	; Tot Qty Ordered    9v4
	RTD_C_RETAIL_W		,D14	; Cust Retail Price 12v2
		;----- Packing Information -------------|
	RTD_PK_W		,D6	; Pack Qty(#ofPack)
	RTD_PK_SIZE_W		,D8	; Qty Per Pack
	RTD_PK_UOFM_W		,A2	; PO4 Unit of Measure
	RTD_PK_WGT_TP_W		,A1	; Pack Weight Type
					; G=Gross,N=Net  Tbl#187 
	RTD_PK_WGT_W		,D8	; Pack Wgt 	6v2
	RTD_PK_UOFM2_W		,A2	; Pack Wgt? UofM
	RTD_PK_DESCR_W		,A80	; Pack Description/Text
		;-- P/O Change Information -------------|
	RTD_CHANGE_TYPE_W	,A2	; Line Item Change Type	
	RTD_SIZE_CODE_W		,A8	; Buyer's Size Code
	RTD_RECV_QTY_W		,D13	; Qty Left To Receive
		;-- PO3 Additional Information ---------|
		;-- Variations in Price Structue -------|
	RTD_PO3_CNG_REAS_W	,A2	; Change Reason Cd T#371
	RTD_PO3_DATE_W		,D6	; Po3 Date
	RTD_PO3_PRICE_QL_W	,A3	; PO3 Price Qual Cd #236
	RTD_PO3_UNIT_PRI_W	,D12	; Unit Price 8v4
	RTD_PO3_PRI_BASIS_W	,A2	; Basis Unit Price #639
	RTD_PO3_QTY_W		,D13	; PO3 Quantity 9v4
	RTD_PO3_UOFM_W		,A2	; Unit of Meas Code #355
	RTD_PO3_DESCR_W		,A80	; Description
	RTD_COLOR_W		,A10	; Product id Color
	RTD_CUST_ITM_QL_W	,A2	; Customer Partno Qual
	RTD_AMOUNT_QL_W		,A2	; Amount qualifiers
	RTD_AMOUNT_W		,D10	; Amounts 8v2
				,A178	; Filler
	RTD_PCNT_W		,D3.2	;

RECORD, X
				,A8	; Company Code
				,D5	; Sequence Number
	RT850U_DTL		,A%SIZE(RT850_DTL)	; Unsplit Detail

.ENDC

.IFDEF XX850_DTL_TOTALS
RECORD RT850_DTL_TOTALS
;
;	Retail Detail - Split Processing. 
;	Working Storage Buffer of Quantity Fields.
;
;	The quantity fields accumulate the total quantities that have been
;	split to multiple companies.  This buffer is not written to any file.
;
	RTD_PCNT_T	,D3.2	; Percentage totals
	RTD_ORD_QTY_T	,D13	; Order Qty Total
.ENDC
