; AO997_HDR.DEF
;
;	Functional Acknowledgment Header -- Functional Group Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	21-MAY-92	T. Morbach	Original
;
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD AO997_HDR
	CFH_GROUP_ID	,A2	; Functional Group Identifier	|   1 thru 2
	CFH_DATA_INT	,D9	; Data Interchange Control #	|   3 thru 11

	CFH_ACK_CODE	,A1	; Acknowledgement Code		|  12 thru 12
	CFH_INCLUDED	,D6	; Number of Trans Sets Included	|  13 thru 18
	CFH_RECVD	,D6	; Number of Trans Sets Received	|  19 thru 24
	CFH_ACCEPTED	,D6	; Number of Trans Set Accepted	|  25 thru 30

	CFH_NOTE_1	,D3	; Group Note Code-1		|  31 thru 33
	CFH_NOTE_2	,D3	; Group Note Code-2		|  34 thru 36
	CFH_NOTE_3	,D3	; Group Note Code-3		|  37 thru 39
	CFH_NOTE_4	,D3	; Group Note Code-4		|  40 thru 42
	CFH_NOTE_5	,D3	; Group Note Code-5		|  43 thru 45
			,A55	; Filler			|  46 thru 100
RECORD CFH_KEY, X
	K_CFH_GROUP_ID	,A%SIZE(CFH_GROUP_ID)
	K_CFH_DATA_INT	,D%SIZE(CFH_DATA_INT)
