; TO997_DTL.DEF
;
;	Functional Acknowledgment Detail - Transaction Set Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	21-DEC-92	T. Bunker	Original
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD TO997_DTL
	TAD_SUPPL_ID	,A15	; Supplier ID			|   1 thru 15
	TAD_DEST_CODE	,A15	; Destination Code		|  16 thru 30
	TAD_GROUP_ID	,A2	; Functional Group Identifier	|  31 thru 32
	TAD_DATA_INT	,D9	; Data Interchange Control #	|  33 thru 41
	TAD_TRANS_TYPE	,A3	; Transaction Set Identifier	|  42 thru 44
	TAD_TRANS_CTL	,D9	; Trasnaction Set Control #	|  45 thru 53

	TAD_ACK_CODE	,A1	; Transaction Set Ack Code	|  54 thru 54
	TAD_NOTE_1	,A3	; Transaction Set Note Code-1	|  55 thru 57
	TAD_NOTE_2	,A3	; Transaction Set Note Code-2	|  58 thru 60
	TAD_NOTE_3	,A3	; Transaction Set Note Code-3	|  61 thru 63
	TAD_NOTE_4	,A3	; Transaction Set Note Code-4	|  64 thru 66
	TAD_NOTE_5	,A3	; Transaction Set Note Code-5	|  67 thru 69
			,A31	; Filler			|  70 thru 100

RECORD TAD_KEY, X
	K_TAD_SUPPL_ID		,A%SIZE(TAD_SUPPL_ID)
	K_TAD_DEST_CODE		,A%SIZE(TAD_DEST_CODE)
	K_TAD_GROUP_ID		,A%SIZE(TAD_GROUP_ID)
	K_TAD_DATA_INT		,D%SIZE(TAD_DATA_INT)
	K_TAD_TRANS_TYPE	,A%SIZE(TAD_TRANS_TYPE)
	K_TAD_TRANS_CTL		,D%SIZE(TAD_TRANS_CTL)
