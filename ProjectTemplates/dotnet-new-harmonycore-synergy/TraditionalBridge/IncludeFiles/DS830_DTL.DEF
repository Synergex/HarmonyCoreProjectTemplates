;DS830_DTL.DEF                                                                                                                      
;                                                                                                                                   
;       DS MATERIAL RELEASE DETAILS                                                                                                 
;       DATE : 03-JAN-01                                                                                                            
;       File Organization : SEQUENTIAL                                                                                              
;       Record Size :   155                                                              Field Positions                           
;-----------------------------------------------------------------------------------------------------------------------------------
RECORD  DS830_DTL                                                                                                                   
        DSD_SUPP_ID               ,A15          ; SUPPLIER ID                                 1 thru    15                          
        DSD_RELNO                 ,A6           ; RELEASE NUMBER                             16 thru    21                          
        DSD_RELDAT                ,D8           ; RELEASE DATE                               22 thru    29                          
        DSD_PARTNO                ,A30          ; DIAMOND STAR PART NUMBER                   30 thru    59                          
        DSD_DEST_ID               ,A15          ; SHIP TO DESTINATION                        60 thru    74                          
        DSD_ISSUE_BY              ,A6           ; DIVISION                                   75 thru    80                          
        DSD_DOCK                  ,A4           ; DOCK CODE                                  81 thru    84                          
        DSD_LINE_FEED             ,A2           ; FACTORY LINE FEED                          85 thru    86                          
        DSD_PO_NO                 ,A10          ; PURCHASE ORDER NUMBER                      87 thru    96                          
        DSD_PRICE_QUOTE           ,A1           ; PRICE QUOTE QL                             97 thru    97                          
        DSD_PURPOSE               ,A2           ; PURPOSE                                    98 thru    99                          
        DSD_REQ_PRD               ,A1           ; REQUIREMENT PERIOD                        100 thru   100                          
        DSD_REQ_TYPE              ,A1           ; REQUIREMENT TYPE                          101 thru   101                          
        DSD_REQ_DATE              ,D8           ; REQUIREMENT DATE                          102 thru   109                          
        DSD_REQ_TIME              ,D4           ; REQUIREMENT TIME                          110 thru   113                          
        DSD_RAN                   ,A7           ; RECEIPT AUTHORIZATION NUMBER              114 thru   120                          
        DSD_THRU_DATE             ,D8           ; REQUIREMENT THRU DATE                     121 thru   128                          
        DSD_REQ_QTY               ,D8           ; REQUIREMENT QUANTITY                      129 thru   136                          
        DSD_GROUP_CD              ,A5           ; GROUP CODE                                137 thru   141                          
        DSD_STOCK_LOC             ,A4           ; STOCK LOCATION                            142 thru   145                          
        DSD_CTN_ID                ,A8           ; CONTAINER ID                              146 thru   153                          
        FILLER                    ,A2           ; FILLER                                    154 thru   155                          
                                                                                                                                    
                                                                                                                                    
RECORD  ,X                                                                                                                          
        DS830_DTL_KEY             ,A120         ; PRIMARY SORT KEY                                                                  
                                  ,A35          ; PRIMARY KEY POSITIONING                                                           
.IFDEF XX830_DTL_WORK
RECORD DS830_DTL_WORK
;
;	DIAMOND STAR Release Detail- Split Processing (Temporary Work File)
;
;	Fields defined in this record are identical to DS830_DTL with the 
;	exception of the additional fields in this record of DSD_COMP_W and 
;	DSD_SEQN_W.
;
;	STRUCTURE:	Sequential
;	LENGTH:		153
;	SORT KEY:	DOD_COMP_W and DSD_SEQN_W
;
	DSD_COMP_W	,A8	; Company Code
	DSD_SEQN_W	,D5	; Sequence Number
	DSD_SUPP_ID_W	,A15	; Supplier ID
	DSD_RELNO_W	,A6	; Release Number
	DSD_RELDAT_W	,D8	; Release Date
	DSD_PARTNO_W	,A30	; Diamond Star Part Number
				; Pos 1-16=Pt#,Pos 17=Eng Change
	DSD_DEST_ID_W	,A15	; Destination - Ship to
	DSD_ISSUE_BY_W	,A6	; Release Issued by (division)
	DSD_DOCK_W	,A4	; Dock Code	
	DSD_LINE_FEED_W	,A2	; Factory Line Feed
	DSD_PO_NO_W	,A10	; Diamond Star Purchase Order #
				; Pos 1-2 = 'P '
				; Pos 3 = Last Digit of Year
				; Pos 4-8  = Unique Number
				; Pos 9-10 = Ammend Level/Status
	DSD_PRICE_QUOTE_W,A1	; 1=Production,2=Service
	DSD_PURPOSE_W	,A2	; '00' = Original Release
				;
	DSD_REQ_PRD_W	,A1	; Requirement Type
				; D=Daily, W=Weekly,F=Flexible
	DSD_REQ_TYPE_W	,A1	; Order Period
				; C=Firm, D=Planning-Tenative
	DSD_REQ_DATE_W	,D8	; Date required
	DSD_REQ_TIME_W	,D4	; Time required
	DSD_RAN_W	,A7	; Receipt Authorization Number
				;
	DSD_THRU_DATE_W	,D8	; Requirement thru date
	DSD_REQ_QTY_W	,D8	; Qty required
	DSD_GROUP_CD_W  ,A5     ; GROUP CODE
	DSD_STOCK_LOC_W ,A4     ; STOCK LOCATION
	DSD_CTN_ID_W    ,A8     ; CONTAINER ID
			,A2	; Filler

RECORD, X
			,A8	; Company Code
			,D5	; Sequence Number
	DS830U_DTL	,A%SIZE(DS830_DTL)	;unsplit detail record
.ENDC

.IFDEF XX830_DTL_TOTALS
RECORD DS830_DTL_TOTALS
;
;	DIAMOND STAR Release requirements - Split Processing. 
;	Working Storage Buffer of Quantity Fields.
;
;	The quantity fields accumulate the total quantities that have been
;	split to multiple companies.  This buffer is not written to any file.
;
	DSD_REQ_QTY_T	,D8	; Qty required

.ENDC

