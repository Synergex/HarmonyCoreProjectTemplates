; HOASN_SNO.DEF
;
;	Honda Maintainable ASN Serial Number File
;
;-------------------------------------------------------------------------------
;--- CARaS Modification Log ----------------------------------------------------
;-------------------------------------------------------------------------------
;
;	Modified	Programmer	Description
;	---------	----------	------------
;	18-Sept-89	J. Rheingans	Original
;	27-FEB-92	M. Farneth	Added HOS_IPP.
;	03-Dec-93	T. Bunker	Expand part number to 30.
;					Expand record from 115 to 128.
;-------------------------------------------------------------------------------
; File Organization: Indexed	| Record Size: 128
;-------------------------------------------------------------------------------

RECORD HOASN_SNO
	HOS_SHIP_ID	,D9	; Shipment (ASN/Shipper) Id Number
	HOS_OUR_PART	,A30	; Our/Supplier Part Number
	HOS_KD_ORDER	,A15	; KD Order Number
	HOS_TARE_LEVEL	,D5	; Tare Level Identifier
	HOS_CONT_CODE	,A5	; Container Code	
	HOS_UNITS_PER	,D7	; Units Per Container	(Detail Containers Only)

	HOS_SERIAL_NO	,A15	; Serial Number
	HOS_RC_SNO	,A15	; Returnable Container Serial Number
	HOS_IPP		,A15	; Initial Production Parts (will contain 'IPP')
			,A12	; Filler
RECORD HOS_KEY0, X
	K0_HOS_SHIP_ID		,D%SIZE(HOS_SHIP_ID)
	K0_HOS_OUR_PART		,A%SIZE(HOS_OUR_PART)
	K0_HOS_KD_ORDER		,A%SIZE(HOS_KD_ORDER)
	K0_HOS_TARE_LEVEL	,D%SIZE(HOS_TARE_LEVEL)
	K0_HOS_CONT_CODE	,A%SIZE(HOS_CONT_CODE)
	K0_HOS_UNITS_PER	,D%SIZE(HOS_UNITS_PER)
	K0_HOS_SERIAL_NO	,A%SIZE(HOS_SERIAL_NO)
	
RECORD HOS_KEY1
	;
	; Do Not Allow Duplicates or Changes
	;
	K1_HOS_SERIAL_NO	,A%SIZE(HOS_SERIAL_NO)

RECORD HOS_KEY2
	;
	; Allow Duplicates (for blanks) but the Maintenance Module will
	; not allow Duplicate non-blanks Serial Numbers, Allow Changes.
	;
	K2_HOS_RC_SNO		,A%SIZE(HOS_RC_SNO)

