; CHRELX_REQ.DEF
;
;	Chrysler X12 Release Requirement File.
;
;----------------------------------------------------------------------
; File Organization: Sequential | Record Size: 100	| Positions
;----------------------------------------------------------------------

RECORD CHRELX_REQ
	CRX_SUPPLIER	,A7	; Supplier Id Code		|   1 thru 7
	CRX_CUST_PART	,A10	; Chrysler Part Number		|   8 thru 17 
	CRX_RELNO	,A6	; Release/Program No.		|  18 thru 23
	CRX_PONUM	,A8	; Purchase Order Number		|  24 thru 31
	CRX_DEST	,A5	; Destination Code		|  32 thru 36
	CRX_REQ_TYPE	,A2	; Requirement Type		|  37 thru 38
				; B=Pre-prod/Pilot,
				; C=Firm/week, D=Monthly
	CRX_REQ_DATE	,D6	; Requirement Date(YYMMDD)	|  39 thru 44
	CRX_REQ_QTY	,D8	; Requirement Quantity		|  45 thru 52
			,A48	; Filler			|  53 thru 100
RECORD CRX_KEY, X
	K_CRX_SUPPLIER	,A%SIZE(CRX_SUPPLIER)
	K_CRX_CUST_PART	,A%SIZE(CRX_CUST_PART)
	K_CRX_RELNO	,A%SIZE(CRX_RELNO)
	K_CRX_PONUM	,A%SIZE(CRX_PONUM)
	K_CRX_DEST	,A%SIZE(CRX_DEST)
	K_CRX_REQ_TYPE	,A%SIZE(CRX_REQ_TYPE)
	K_CRX_REQ_DATE	,D%SIZE(CRX_REQ_DATE)

