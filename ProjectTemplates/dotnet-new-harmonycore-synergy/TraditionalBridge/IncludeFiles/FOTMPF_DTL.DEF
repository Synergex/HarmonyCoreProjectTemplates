; FOTMPF_DTL
;
;	File Definition for the Ford Fixed Length Temporary ASN Detail File.
;	This File is used to create the Actual Transmitted File (FOASN_RJE)
;	to Ford.
;
;	File Organization: Sequential		Record Size: 100
;
RECORD FOTMPF_DTL
	TD_CONV_NO	,A10		; Conveyance Number.
	TD_DEST_CODE	,A5		; Destination Code.
	TD_SHIP_ID	,D9		; Shipment Id Number.
	TD_CUST_PART	,A22		; Ford Part Number.
	TD_OUR_PART	,A15		; CARaS Part Number.
	TD_UOFM		,A2		; Unit of Measure.
	TD_PRIOR_CUM	,D9		; Yesterday's YTD Ship Cum.
	TD_SHIP_QTY	,D7		; Quantity Shipped.
	TD_WEIGHT	,D6		; Gross Weight (in LBs).
	TD_PACK_SLIP	,D6		; Packing Slip Number.
	TD_CONTAINERS	,D3		; Total Quantity for Parts Containers.
			,A6		; Filler.
RECORD, X
	DTL_KEY		,A46		; Sort Key.

RECORD LAST_DTL
			,A46
