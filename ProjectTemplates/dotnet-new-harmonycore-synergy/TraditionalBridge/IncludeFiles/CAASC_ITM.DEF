RECORD CAASC_ITM
	ASCI_TP_DESIG	,A2	; Trading Partner Designator	      | 001-002
	ASCI_SID	,A30	; Original Shipment Id Number	      | 003-032
	ASCI_SID_SEQ	,D2	; Our Sequence No for dups/changes,etc| 033-034
	ASCI_SUPP_IDCD	,A17	; Original Supplier ID code	      | 035-051
	ASCI_SHFR_IDCD	,A17	; Supplier Ship From ID code	      | 052-068
	ASCI_SHTO_IDCD	,A17	; Ship To ID code                     | 069-085
	ASCI_DEST_IDCD	,A17	; Final Dest ID code                  | 086-102
	ASCI_CUST_PART	,A30	; Part No                             | 103-132
	ASCI_PART_SEQ	,D6	; Sequenced as received               | 133-138
				;				      |
	ASCI_PART_QL	,A2	; Part No Qualifier		      | 139-140
				;				      |
	ASCI_PO_NUM	,A22	; Customer Purchase Order Number      | 141-162
	ASCI_CONTRACT	,A30	; Contract Number		      | 163-192
				;				      |
	ASCI_SHIP_QTY	,D8	; Quantity Shipped In 		      | 193-200
	ASCI_YTD_CUM	,D8	; YTD Ship Qty			      | 201-208
	ASCI_UOFM	,A2	; Unit of Measure		      | 209-210
	ASCI_ORDER_QTY	,D8	; Qty Ordered			      | 211-218
	ASCI_ORDER_UOFM	,A2	; Unit of Measure Ordered	      | 219-220
	ASCI_DOCK	,A30	; Dock Code			      | 221-250
				;				      |
	ASCI_PRD_QL	,15A2	; Product Qualifiers		      | 251-280
	ASCI_PRD_ID	,15A30	; Product ID Codes		      | 281-730
				;				      |
	ASCI_SHIP_ID	,D9	; Outbound Shipment ID (ASC#)         | 731-739

		;--- For Update Purposes ---------		      |
	ASCI_TRANS_DATE	,D6	; Date Transmitted, Format: YYMMDD    | 740-745
	ASCI_TRANS_TIME	,D4	; Time Transmitted, Format: HHMM      | 746-749
	ASCI_ISA_CTL	,D9	; Transmitted with ISA Control #      | 750-758
	ASCI_GS_CTL	,D9	; Transmitted with GS  Control #      | 760-767
	ASCI_ST_CTL	,D9	; Transmitted with ST  Control #      | 768-776
	ASCI_TRANS_ASN	,A11	; Transmitted ASN Number	      | 777-787
	ASCI_ASN_PURP	,A2	; Last xmitted purpose code	      | 788-789
	ASCI_XMIT_QTY	,D8	; Quantity transmitted 		      | 790-797
	ASCI_2ND_PURP   ,A2	; Second purpose (ie received cancel) | 798-799
	ASCI_2ND_SID	,A30	; Second ASN/SID cancelling this one  | 800-729
			,A195	; Filler			      | 798-1024

RECORD, X
	ASCI_KEY0 	,A138
;
;	Changes (to Ship_id only), No duplicates
;
RECORD	ASCI_KEY1
	K1_ASCI_SHIP_ID		,D%SIZE(ASCI_SHIP_ID)
	K1_ASCI_TP_DESIG	,A%SIZE(ASCI_TP_DESIG)
	K1_ASCI_SID		,A%SIZE(ASCI_SID)
	K1_ASCI_SID_SEQ		,D%SIZE(ASCI_SID_SEQ)
	K1_ASCI_SUPP_IDCD	,A%SIZE(ASCI_SUPP_IDCD)
	K1_ASCI_SHFR_IDCD	,A%SIZE(ASCI_SHFR_IDCD)
	K1_ASCI_SHTO_IDCD	,A%SIZE(ASCI_SHTO_IDCD)
	K1_ASCI_DEST_IDCD	,A%SIZE(ASCI_DEST_IDCD)
	K1_ASCI_CUST_PART	,A%SIZE(ASCI_CUST_PART)
	K1_ASCI_PART_SEQ	,D%SIZE(ASCI_PART_SEQ)
