; FORACK_SEG.DEF
;
;	Functional Acknowledgment Detail - Segment Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	09-Jul-89	J. Rheingans	Original
;
;	01-Jun-90	Rich Mae	Added Supplier ID and Destination Code.
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD FORACK_SEG
	FAS_SUPPL_ID	,A15	; Supplier ID			|   1 thru 15
	FAS_DEST_CODE	,A15	; Destination Code		|  16 thru 30
	FAS_GROUP_ID	,A2	; Functional Group Identifier	|  31 thru 32
	FAS_DATA_INT	,D9	; Data Interchange Control #	|  33 thru 41
	FAS_TRANS_TYPE	,A3	; Transaction Set Identifier	|  42 thru 44
	FAS_TRANS_CTL	,D9	; Trasnaction Set Control #	|  45 thru 53
	FAS_SEGMENT	,A3	; Segment Identifier		|  54 thru 56
	FAS_LOOP_ID	,A4	; Loop Identifier		|  57 thru 60

	FAS_NOTE_1	,A3	; Segment Note Code-1		|  61 thru 63
	FAS_NOTE_2	,A3	; Segment Note Code-2		|  64 thru 66
	FAS_NOTE_3	,A3	; Segment Note Code-3		|  67 thru 69
	FAS_NOTE_4	,A3	; Segment Note Code-4		|  70 thru 72
	FAS_NOTE_5	,A3	; Segment Note Code-5		|  73 thru 75
			,A25	; Filler			|  76 thru 100

RECORD FAS_KEY, X
	K_FAS_SUPPL_ID	        ,A%SIZE(FAS_SUPPL_ID)
	K_FAS_DEST_CODE	        ,A%SIZE(FAS_DEST_CODE)
	K_FAS_GROUP_ID		,A%SIZE(FAS_GROUP_ID)
	K_FAS_DATA_INT		,D%SIZE(FAS_DATA_INT)
	K_FAS_TRANS_TYPE	,A%SIZE(FAS_TRANS_TYPE)
	K_FAS_TRANS_CTL		,D%SIZE(FAS_TRANS_CTL)
	K_FAS_SEGMENT		,A%SIZE(FAS_SEGMENT)
	K_FAS_LOOP_ID		,D%SIZE(FAS_LOOP_ID)

