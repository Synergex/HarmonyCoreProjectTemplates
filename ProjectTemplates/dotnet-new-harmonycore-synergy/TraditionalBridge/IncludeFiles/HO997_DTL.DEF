; HO997_DTL.DEF
;
;	Functional Acknowledgment Detail - Transaction Set Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	29-Oct-90	J. Curl		Original
;
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD HO997_DTL
	HFD_GROUP_ID	,A2	; Functional Group Identifier	|   1 thru 2
	HFD_DATA_INT	,D9	; Data Interchange Control #	|   3 thru 11
	HFD_TRANS_TYPE	,A3	; Transaction Set Identifier	|  12 thru 14
	HFD_TRANS_CTL	,D9	; Trasnaction Set Control #	|  15 thru 23

	HFD_ACK_CODE	,A1	; Transaction Set Ack Code	|  24 thru 24
	HFD_NOTE_1	,D3	; Transaction Set Note Code-1	|  25 thru 27
	HFD_NOTE_2	,D3	; Transaction Set Note Code-2	|  28 thru 30
	HFD_NOTE_3	,D3	; Transaction Set Note Code-3	|  31 thru 33
	HFD_NOTE_4	,D3	; Transaction Set Note Code-4	|  34 thru 36
	HFD_NOTE_5	,D3	; Transaction Set Note Code-5	|  37 thru 38
			,A62	; Filler			|  39 thru 100
RECORD HFD_KEY, X
	K_HFD_GROUP_ID		,A%SIZE(HFD_GROUP_ID)
	K_HFD_DATA_INT		,D%SIZE(HFD_DATA_INT)
	K_HFD_TRANS_TYPE	,A%SIZE(HFD_TRANS_TYPE)
	K_HFD_TRANS_CTL		,D%SIZE(HFD_TRANS_CTL)
