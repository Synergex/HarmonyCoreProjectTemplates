;
;	Nissan Advance Shipping Notice (ASN) Header
;
;	File Organization: Isam		Record Size: 130
;
;	Primary Key --> NIH_SHIP_ID (Positions 1 thru 9)
;
;		 	Note: If FFH_SHIP_ID.EQ.000000000
;			      Then	Allow Duplicates (A Default Record)
;
;			      Else	Do Not Allow Duplicate Ship Ids.
;
;-------------------------------------------------------------------------------
;	Modified	Programmer	Description
;	--------	----------	-----------
;	10-Nov-1988	Gene Scibilia	Creation
;
;-------------------------------------------------------------------------------
;

RECORD NIASN_HDR
	NIH_SHIP_ID		,D9	; Advanced Shipping Notice Number
	NIH_SUPP_ABBRV		,A5	; Supplier's Abbreviation
	NIH_DEST_ABBRV		,A5	; Destination Abbreviation
	NIH_BILL_LADING		,A15	; Bill of Lading Number.

	NIH_EQUIP_CODE		,A2	; Equipment Code
	NIH_EQUIP_PRE		,A4	; Equipment Prefix
	NIH_EQUIP_NO		,A7	; Equipment Number

	NIH_PACK_CODE		,A5	; Packaging Code
	NIH_PACK_QTY		,D8	; Packaged Quantity

	NIH_ASN_DATE		,D6	; A.S.N. Date  -- Format: YYMMDD
	NIH_GROSS_WGT		,D8	; Gross Weight
	NIH_SHIP_DATE		,D6	; Date Shipped -- Format: YYMMDD
	NIH_SHIP_TIME		,D4	; Time Shipped -- Format: HHMM

	NIH_TRANS_DATE		,D6	; Date Transmitted -- Format: YYMMDD
	NIH_TRANS_TIME		,D4	; Time Transmitted -- Format: HHMM
	NIH_ORG_DATE		,D6	; Date of Original Transmission.
	NIH_ORG_TIME		,D4	; Time of Original Transmission.
	NIH_CNTRL_NO		,D9	; ICS Control Number
					;	( Transmission Control # )
				,A17	; Filler	

;-------------------------------------------------------------------------------
;	       ------- Alternate Key Definitions ---------
;-------------------------------------------------------------------------------

RECORD NIH_KEY1
	;
	; No Duplicates, No Changes Allowed.
	;
	K1_NIH_SHIP_ID		,D9	; Ship Identification Number.
	K1_NIH_DEST_ABBRV	,A5	; Destination Abbreviation.

RECORD NIH_KEY2
	;
	; No Duplicates, Changes Allowed.
	;
	K2_NIH_SUPP_ABBRV	,A5	; Supplier Abbreviation
	K2_NIH_SHIP_ID		,D9	; Ship Identification

;-------------------------------------------------------------------------------
;              ------- Alternate Record Defintion -------
;-------------------------------------------------------------------------------

.IFDEF NIASN_HDR_ALT
RECORD A_NIASN_HDR
			,A%SIZE(NIASN_HDR)
RECORD,X
	A_NIH_SHIP_ID		,D9	; Advanced Shipping Notice Number
	A_NIH_SUPP_ABBRV	,A5	; Supplier's Abbreviation
	A_NIH_DEST_ABBRV	,A5	; Destination Abbreviation
	A_NIH_BILL_LADING	,A15	; Bill of Lading Number.

	A_NIH_EQUIP_CODE	,A2	; Equipment Code
	A_NIH_EQUIP_PRE		,A4	; Equipment Prefix
	A_NIH_EQUIP_NO		,A7	; Equipment Number

	A_NIH_PACK_CODE		,A5	; Packaging Code
	A_NIH_PACK_QTY		,D8	; Packaged Quantity

	A_NIH_ASN_DATE		,D6	; A.S.N. Date  -- Format: YYMMDD
	A_NIH_GROSS_WGT		,D8	; Gross Weight
	A_NIH_SHIP_DATE		,D6	; Date Shipped -- Format: YYMMDD
	A_NIH_SHIP_TIME		,D4	; Time Shipped -- Format: HHMM

	A_NIH_TRANS_DATE	,D6	; Date Transmitted -- Format: YYMMDD
	A_NIH_TRANS_TIME	,D4	; Time Transmitted -- Format: HHMM
	A_NIH_ORG_DATE		,D6	; Date of Original Transmission.
	A_NIH_ORG_TIME		,D4	; Time of Original Transmission.
	A_NIH_CNTRL_NO		,D9	; ICS Control Number
					;	( Transmission Control # )
				,A17	; Filler	
.ENDC
