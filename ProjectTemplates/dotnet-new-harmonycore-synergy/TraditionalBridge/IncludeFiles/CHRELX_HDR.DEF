; CHRELX_HDR.DEF
;
;	Chrysler X12 Release Header File.
;
;------------------------------------------------------------------
; File Organization: Sequential | Record Size: 512	| Positions
;------------------------------------------------------------------

RECORD CHRELX_HDR
	CHX_SUPPLIER	,A7	; Supplier Id Code	| 001 thru 007
	CHX_CUST_PART	,A10	; Chrysler Part Number	| 008 thru 017
	CHX_RELNO	,A6	; Release/Program No.	| 018 thru 023
	CHX_PONUM	,A8	; Purchase Order Number	| 024 thru 031
	CHX_ISSUE_BY	,A5	; Issue By Code		| 032 thru 036
	CHX_PART_NOTE	,A35	; Part Release Note	| 037 thru 071
	CHX_UOFM	,A2	; Unit of Measure	| 072 thru 073
	CHX_CONTROL	,A12	; Local/Group Control 	| 074 thru 085
	CHX_DECK	,A4	; Deck Code		| 086 thru 089
	CHX_CONTACT	,A35	; Chrysler Contact	| 090 thru 124
	CHX_PHONE	,A10	; Chrysler Contact Phone| 125 thru 134
	CHX_ISSUE_DATE	,D6	; Issue/Release Date	| 135 thru 140
	CHX_FAB_QTY	,D8	; Authorized Fab Qty	| 141 thru 148
	CHX_FAB_DATE	,D6	; Authorized Fab Date	| 149 thru 154
	CHX_RAW_QTY	,D8	; Authorized Raw Qty	| 155 thru 162
	CHX_RAW_DATE	,D6	; Authorized Raw Date	| 163 thru 168
	CHX_NOTE_ARRAY	,6A51	; Release Notes		| 169 thru 474
	CHX_HORIZ_START	,D6	; Horizon Start Date	| 475 thru 480
	CHX_HORIZ_END	,D6	; Horizon End Date	| 481 thru 486
	CHX_DUAL_SERIES	,D1	; Flag to indicate dual | 487 thru 487
				; series
			,A25	; Filler		| 488 thru 512

RECORD CHX_KEY		,X
	K_CHX_SUPPLIER		,A%SIZE(CHX_SUPPLIER)
	K_CHX_CUST_PART		,A%SIZE(CHX_CUST_PART)
	K_CHX_RELNO		,A%SIZE(CHX_RELNO)
	K_CHX_PONUM		,A%SIZE(CHX_PONUM)

RECORD CHX_KEY_1	,X
	K1_CHX_SUPPLIER		,A%SIZE(CHX_SUPPLIER)
	K1_CHX_CUST_PART	,A%SIZE(CHX_CUST_PART)

