; FOREL8_NTE.DEF
;
;	FORD TCD/EED X12 RELEASE NOTES FILE
;
;	DATE: 03-OCT-88
;
;----------------------------------------------------------------------------------------------------
;	FILE ORGANIZATION: SEQUENTIAL   |     RECORD SIZE: 128		|  	FIELD POSITIONS
;----------------------------------------------------------------------------------------------------
;	02-Dec-93	T. Bunker		Expand part number to 30
;						Expand record from 112 to 128
;----------------------------------------------------------------------------------------------------

RECORD FOREL8_NTE
	FN8_PARTNO	,A30		; Ford Part Number		|	1 thru 30
	FN8_RELNO	,A5		; Release Number		|      31 thru 35
	FN8_DEST_CODE	,A5		; Destination Code		|      36 thru 40
	FN8_NOTE_NO	,D5		; Note Number			|      41 thru 45
	FN8_NOTE	,A60		; Ford Release Note 		|      46 thru 105
	FN8_SUPP	,A5		; Supplier Id			|     106 thru 110
			,A18		; Filler			|     111 thru 128
;
RECORD	FN8_KEY
	FN8_SUPP_K		,A%SIZE(FN8_SUPP)	; Supplier ID
	FN8_PARTNO_K		,A%SIZE(FN8_PARTNO)	; Ford Part Number
	FN8_RELNO_K		,A%SIZE(FN8_RELNO)	; Release Number
	FN8_DEST_CODE_K		,A%SIZE(FN8_DEST_CODE)	; Destination Code

.IFDEF INC_FORL8U_NTE_WORK
RECORD FORL8U_NTE_WORK
;
	FN8_COMP_W	,A8		;COMPANY CODE
	FN8_SEQN_W	,D5		;SEQUENCE NUMBER
	FN8_PARTNO_W	,A30		; Ford Part Number
	FN8_RELNO_W	,A5		; Release Number
	FN8_DEST_CODE_W	,A5		; Destination Code
	FN8_NOTE_NO_W	,D5		; Note Number
	FN8_NOTE_W	,A60		; Ford Release Note
	FN8_SUPP_W	,A5		; Supplier Id
			,A18		; Filler
RECORD	,X
			,A8	;COMPANY_CODE
			,D5	;SEQUENCE NUMBER
	FORL8_NTE	,A%SIZE(FOREL8_NTE)
.ENDC
