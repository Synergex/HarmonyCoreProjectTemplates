; Document Journal enevelope standard literals
	DJL_X12			,A1,'X'
	DJL_ODETTE		,A1,'O'
	DJL_EDIFACT		,A1,'E'
	DJL_UCS			,A1,'U'
	DJL_XML			,A1,'M'
	DJL_OTHER		,A1,'?'
	DJL_VDA			,A1,'V'
	DJL_TRADACOM		,A1,'T'

;-------------------------------------------------------------------------------
; Document Journal record selected literals
	DJSEL_YES		,D1,1
	DJSEL_NO		,D1,5

