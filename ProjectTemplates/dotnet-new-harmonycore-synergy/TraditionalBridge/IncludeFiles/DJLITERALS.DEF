		; The following are valid for DJ3_RT
	DJRT_APPLICATION	,A2,'60'
	DJRT_EDI		,A2,'62'
	DJRT_EDI_MULTIPLE	,A2,'63' ; Multiple EDI files combined into
					 ; single file
