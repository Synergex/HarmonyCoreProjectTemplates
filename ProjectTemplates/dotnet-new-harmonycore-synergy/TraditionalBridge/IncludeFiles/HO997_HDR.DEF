; HO997_HDR.DEF
;
;	Functional Acknowledgment Header -- Functional Group Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	29-Oct-90	J. Curl		Original
;
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD HO997_HDR
	HFH_GROUP_ID	,A2	; Functional Group Identifier	|   1 thru 2
	HFH_DATA_INT	,D9	; Data Interchange Control #	|   3 thru 11

	HFH_ACK_CODE	,A1	; Acknowledgement Code		|  12 thru 12
	HFH_INCLUDED	,D6	; Number of Trans Sets Included	|  13 thru 18
	HFH_RECVD	,D6	; Number of Trans Sets Received	|  19 thru 24
	HFH_ACCEPTED	,D6	; Number of Trans Set Accepted	|  25 thru 30

	HFH_NOTE_1	,D3	; Group Note Code-1		|  31 thru 33
	HFH_NOTE_2	,D3	; Group Note Code-2		|  34 thru 36
	HFH_NOTE_3	,D3	; Group Note Code-3		|  37 thru 39
	HFH_NOTE_4	,D3	; Group Note Code-4		|  40 thru 42
	HFH_NOTE_5	,D3	; Group Note Code-5		|  43 thru 45
			,A55	; Filler			|  46 thru 100
RECORD HFH_KEY, X
	K_HFH_GROUP_ID	,A%SIZE(HFH_GROUP_ID)
	K_HFH_DATA_INT	,D%SIZE(HFH_DATA_INT)
