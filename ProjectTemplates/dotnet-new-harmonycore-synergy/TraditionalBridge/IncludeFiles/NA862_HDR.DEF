        NSH_RECV_DATE_W         ,D8           ; LAST RECEIVE DATE 
        NSH_RECV_QTY_W          ,D8           ; LAST RECEIVE QUANTITY
        NSH_YTD_CUM_W           ,D9           ; YTD CUM QUANTITY
        NSH_CUM_START_W         ,D8           ; CUM START DATE
        NSH_CUM_END_W           ,D8           ; CUM END DATE 
        NSH_SHIP_CODE_W         ,A2           ; SHIP CODE  
