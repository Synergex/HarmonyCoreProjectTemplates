RECORD, X
                        	,A8           ; Company Code
                        	,D5           ; Sequence Number
        NA862U_HDR      ,A%SIZE(NA862_HDR)    ; Unsplit Header
.ENDC

.IFDEF XX862_HDR_TOTALS
RECORD NA862_HDR_TOTALS
;
;	Navistar Release Header - Split Processing
;	Working Storage Buffer of Quantity Fields.
;
;	The quantity fields accumulate the total quantities that have been 
;	split to multiple companies.  This buffer is not written to any file.
;
	NSH_YTD_CUM_T		  ,D8		; CUM RECEIVED TO DATE
	NSH_PCNT_T		  ,D3.2		; PERCENT OF SPLIT
.ENDC
