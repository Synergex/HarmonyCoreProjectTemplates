; GM856_DTL.DEF
;
;	General Motors Raw Input Detail File for X12 ASN Generation.
;
;------------------------------------------------------------------
; File Organization: Sequential | Record Size: 200	| Positions
;------------------------------------------------------------------

RECORD GM856_DTL
	GD_INPUT_SEQ	,D5	; Input Sequence	|   1 thru 5
	GD_ISSUE_BY	,A15	; Issued By (Div) Code	|   6 thru 20
	GD_SHIP_TO	,A15	; Ship To Dest Code	|  21 thru 35
	GD_ASN_NO	,D9	; ASN Number		|  36 thru 44
	GD_PART_NO	,A8	; GM Part Number	|  45 thru 52

	GD_REL_QUAL	,A2	; Release Number Qual	|  53 thru 54
	GD_REL_NO	,A15	; Release Number	|  55 thru 69

	GD_PO_QUAL	,A2	; Purchase Order Qual	|  70 thru 71
	GD_PO_NO	,A9	; Purchase Order No	|  72 thru 80

	GD_RETRN_QUAL	,A2	; Returnable Containers	|  81 thru 82
	GD_RETRN_QTY	,A8	; Returnable Cont Qty	|  83 thru 90
	GD_RETRN_CODE	,A1	; Returnable Cont Code	|  91 thru 91

	GD_SHIP_QTY	,D8	; Ship Quantity		|  92 thru 99
	GD_UOFM		,A2	; Unit of Measure	| 100 thru 101
	GD_YTD_CUM	,D9	; Year To Date Ship Qty	| 102 thru 110
	
	GD_PACK_CODE	,A5	; Packaging Code	| 111 thru 115
	GD_PACK_QTY	,D6	; Number of Containers	| 116 thru 121
	
	GD_WEIGHT_QUAL1	,A2	; Weight Qualifier-1	| 122 thru 123
	GD_WEIGHT	,D8	; Weight		| 124 thru 131
	GD_WEIGHT_QUAL2	,A2	; Weight Qualifier-2	| 132 thru 133

	GD_LOT_QUAL	,A2	; Lot Number Qualifier	| 134 thru 135
	GD_LOT_NO	,A15	; Lot Number		| 136 thru 150

	GD_HEAT_QUAL	,A2	; Heat Code Qualifier	| 151 thru 152
	GD_HEAT_CODE	,A15	; Heat Code Number	| 153 thru 167
			,A33	; Filler		| 168 thru 200
