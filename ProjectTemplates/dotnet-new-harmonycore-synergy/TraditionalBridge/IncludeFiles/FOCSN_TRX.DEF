; FOCSN_TRX.DEF
;
;	Ford CMMS Container/Serial Number Transaction File Definition.
;
;	File Organization: Indexed (Isam)
;	      Record Size: 100 bytes.
;
;	Date: 11-Apr-89
;
RECORD FOCSN_TRX
	FT_SHIP_ID	,D9	; CARaS ASN/Shipper Number.
	FT_OUR_PART	,A22	; CARaS Part Number (in Part Master)
	FT_CONT_CODE	,A5	; Container Code.
	FT_UNITS_PER	,D7	; Units Per Container.
	FT_SERIAL_NO	,A15	; Shipping Label Serial Number.
			,A42	; Filler.
RECORD, X
	FT_KEY0		,A58	; Primary Key (Duplicates Allowed)
