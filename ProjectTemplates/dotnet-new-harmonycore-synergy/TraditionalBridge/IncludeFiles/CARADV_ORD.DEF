RECORD CARADV_ORD
;
; CARADV_ORD - CARaS Receiving Advice Order/Destination level file
;
;-------------------------------------------------------------------------------
;	Record Length: 512	Organization: Indexed		| Positions
;-------------------------------------------------------------------------------
				;				|
	RADO_TP_DESIG	,A2	; Trading Partner Designator	| 001 thru 002
	RADO_RCVADV_NO	,A30	; Receiving Advice ID Number	| 003 thru 032
	RADO_DEST_ID	,A17	; Final Dest ID Code		| 033 thru 049 
	RADO_DEST_ABRV	,A5	; Final Dest Abbr		| 050 thru 054
				;				|
	RADO_DEST_IDQL	,A2	; Final Dest ID Code Qual	| 055 thru 056
	RADO_DEST_BOL	,A30	; ASN BOL # for the Dest	| 057 thru 086
				;				|
			,A426	; Filler			| 087 thru 512
;
;-------------------------------------------------------------------------------
;              ------ Key Definitions ------
;-------------------------------------------------------------------------------
; Primary Key - No Dups, No Changes
;
RECORD,X
	RADO_KEY0	,A49
	
