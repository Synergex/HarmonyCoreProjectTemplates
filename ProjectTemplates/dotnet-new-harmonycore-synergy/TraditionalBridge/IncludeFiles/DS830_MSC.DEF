;DS830_MSC.DEF                                                                                                                      
;                                                                                                                                   
;       DS MATERIAL RELEASE MISC                                                                                                    
;       DATE : 17-JAN-05                                                                                                            
;       File Organization : SEQUENTIAL                                                                                              
;       Record Size :   188                                                               Field Positions                           
;-----------------------------------------------------------------------------------------------------------------------------------
RECORD  DS830_MSC                                                                                                                   
        DSM_SUPP_ID               ,A15          ; SUPPLIER ID                                 1 thru    15                          
        DSM_RELNO                 ,A6           ; RELEASE NUMBER                             16 thru    21                          
        DSM_RELDAT                ,D8           ; RELEASE DATE                               22 thru    29                          
        DSM_PARTNO                ,A30          ; DIAMOND STAR PART NUMBER                   30 thru    59                          
        DSM_DEST_ID               ,A15          ; SHIP TO DESTINATION                        60 thru    74                          
        DSM_ISSUE_BY              ,A6           ; DIVISION                                   75 thru    80                          
        DSM_DOCK                  ,A4           ; DOCK CODE                                  81 thru    84                          
        DSM_LINE_FEED             ,A2           ; FACTORY LINE FEED                          85 thru    86                          
        DSM_PO_NO                 ,A10          ; PURCHASE ORDER NUMBER                      87 thru    96                          
        DSM_PRICE_QUOTE           ,A1           ; PRICE QUOTATION CODE                       97 thru    97                          
        DSM_PURPOSE               ,A2           ; TRANS SET PURPOSE CODE                     98 thru    99                          
        DSM_SRT_FLD               ,D10          ; SORT FIELD                                100 thru   109                          
        DSM_SEG_TYPE              ,A3           ; SEGMENT TYPE                              110 thru   112                          
        DSM_QTY_QL                ,A2           ; SHP QUANITITY QUALIFIER                   113 thru   114                          
        DSM_QUANTITY              ,D10          ; QUANTITY RECEIVED                         115 thru   124                          
        DSM_DTM_QL                ,A3           ; DATE TIME QUALIFIER                       125 thru   127                          
        DSM_DTM_DATE              ,D8           ; DATE                                      128 thru   135                          
        DSM_REF_QL                ,A3           ; REFERENCE NUMBER QUALIFIER                136 thru   138                          
        DSM_REF_NUMBER            ,A30          ; REFERENCE NUMBER                          139 thru   168                          
        DSM_ATH_CODE              ,A2           ; RESOURCE AUTHORIZATION CODE               169 thru   170                          
        DSM_ATH_DATE              ,D8           ; AUTHORIZATION DATE                        171 thru   178                          
        DSM_ATH_QTY               ,D10          ; AUTHORIZATION QUANTITY                    179 thru   188                          
                                                                                                                                    
                                                                                                                                    
RECORD  ,X                                                                                                                          
        DS830_MSC_KEY             ,A112         ; PRIMARY SORT KEY                                                                  
                                  ,A76          ; PRIMARY KEY POSITIONING
.IFDEF XX830_MSC_WORK
RECORD DS830_MSC_WORK

;	DIAMOND STAR Release Misc - Split Processing (Temporary Work File)
;
;	Fields defined in this record are identical to TO830_MSC with the 
;	exception of the additional fields in this record of DSM_COMP_W, 
;	DSM_SEQN_W and DSM_PCNT_W.
;
;	STRUCTURE:	Sequential
;	LENGTH:		204
;	SORT KEY:	DSM_COMP_W and DSM_SEQN_W
;
	DSM_COMP_W	 ,A8	; Company Code
	DSM_SEQN_W	 ,D5	; Sequence Number
        DSM_SUPP_ID_W    ,A15          ; SUPPLIER ID
        DSM_RELNO_W      ,A6           ; RELEASE NUMBER
        DSM_RELDAT_W     ,D8           ; RELEASE DATE
        DSM_PARTNO_W     ,A30          ; DIAMOND STAR PART NUMBER
        DSM_DEST_ID_W    ,A15          ; SHIP TO DESTINATION
        DSM_ISSUE_BY_W   ,A6           ; DIVISION
        DSM_DOCK_W       ,A4           ; DOCK CODE
        DSM_LINE_FEED_W  ,A2           ; FACTORY LINE FEED
        DSM_PO_NO_W      ,A10          ; PURCHASE ORDER NUMBER
        DSM_PRICE_QUOTE_W,A1           ; PRICE QUOTATION CODE
        DSM_PURPOSE_W    ,A2           ; TRANS SET PURPOSE CODE
        DSM_SRT_FLD_W    ,D10          ; SORT FIELD
        DSM_SEG_TYPE_W   ,A3           ; SEGMENT TYPE
        DSM_QTY_QL_W     ,A2           ; SHP QUANITITY QUALIFIER
        DSM_QUANTITY_W   ,D10          ; QUANTITY RECEIVED
        DSM_DTM_QL_W     ,A3           ; DATE TIME QUALIFIER
        DSM_DTM_DATE_W   ,D8           ; DATE
        DSM_REF_QL_W     ,A3           ; REFERENCE NUMBER QUALIFIER
        DSM_REF_NUMBER_W ,A30          ; REFERENCE NUMBER
        DSM_ATH_CODE_W   ,A2           ; RESOURCE AUTHORIZATION CODE
        DSM_ATH_DATE_W   ,D8           ; AUTHORIZATION DATE
        DSM_ATH_QTY_W    ,D10          ; AUTHORIZATION QUANTITY

RECORD, X
			,A8	; Company Code
			,D5	; Sequence Number
	DS830U_MSC	,A%SIZE(DS830_MSC)	;unsplit header record
.ENDC


.IFDEF XX830_MSC_TOTALS
RECORD DS830_MSC_TOTALS
;
;	Diamond Star Release Misc - Split Processing. 
;	Working Storage Buffer of Percentage Fields.
;
;	The quantity fields accumulate the total quantities that have been
;	split to multiple companies.  This buffer is not written to any file.
;
	DSM_PCNT_T	,D3.2	; Percent of total split
.ENDC
