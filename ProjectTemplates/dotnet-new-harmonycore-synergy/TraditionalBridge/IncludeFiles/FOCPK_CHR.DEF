	CPC_RANGE_MIN		,D10		; Minimum Range 
	CPC_CPK_CAPABILITY	,D10		; CPK Capability 
	CPC_X_DOUBLE_BAR	,D10		; X Double Bar 
	CPC_R_BAR		,D10		; R Bar 
	CPC_STD_DEVIATION	,D10		; Standard Deviation 
	CPC_CP_CAPABILITY	,D10		; CP Capability 
						; 
