;DS830_HDR.DEF                                                                                                                      
;                                                                                                                                   
;       DS MATERIAL RELEASE HEADER                                                                                                  
;       DATE : 17-JAN-05                                                                                                            
;       File Organization : SEQUENTIAL                                                                                              
;       Record Size :   420                                                               Field Positions                           
;-----------------------------------------------------------------------------------------------------------------------------------
RECORD  DS830_HDR                                                                                                                   
        DSH_SUPP_ID               ,A15          ; SUPPLIER ID                                 1 thru    15                          
        DSH_RELNO                 ,A6           ; RELEASE NUMBER                             16 thru    21                          
        DSH_RELDAT                ,D8           ; RELEASE DATE                               22 thru    29                          
        DSH_PARTNO                ,A30          ; DIAMOND STAR PART NUMBER                   30 thru    59                          
        DSH_DEST_ID               ,A15          ; SHIP TO DESTINATION                        60 thru    74                          
        DSH_ISSUE_BY              ,A6           ; DIVISION                                   75 thru    80                          
        DSH_DOCK                  ,A4           ; DOCK CODE                                  81 thru    83                          
        DSH_LINE_FEED             ,A2           ; FACTORY LINE FEED                          85 thru    86                          
        DSH_PO_NO                 ,A10          ; PURCHASE ORDER NUMBER                      87 thru    96                          
        DSH_PRICE_QUOTE           ,A1           ; PRICE QUOTATION CODE                       97 thru    97                          
        DSH_PURPOSE               ,A2           ; TRANS SET PURPOSE CODE                     98 thru    99                          
        DSH_DELIV_CODE            ,A2           ; DELIVERY CODE                             100 thru   101                          
        DSH_FROM_DATE             ,D8           ; HORIZON START DATE                        102 thru   109                          
        DSH_TO_DATE               ,D8           ; HORIZON END DATE                          110 thru   117                          
        DSH_QTY_IND               ,A1           ; QUANTITY SENT QL                          118 thru   118                          
        DSH_PART_DESCR            ,A30          ; PART DESCRIPTION                          119 thru   148                          
        DSH_PKG_CODE              ,A5           ; PACKAGING CONTAINER CODE                  149 thru   153                          
        DSH_PKG_QTY               ,D6           ; NUMBER OF PACKAGES                        154 thru   159                          
        DSH_UOFM                  ,A2           ; UNIT OF MEASURE                           160 thru   161                          
        DSH_EXPEDITOR             ,A2           ; EXPEDITOR                                 162 thru   163                          
        DSH_DEL_PATTERN           ,A2           ; DELIVERY PATTERN                          164 thru   165                          
        DSH_DEL_TIME              ,A1           ; SHIPMENT DELIVERY TIME                    166 thru   166                          
        DSH_CANCEL_REF            ,A30          ; CANCELLATION REFERENCE NUMBER             167 thru   196                          
        DSH_TRANSNUM              ,A5           ; DOCUMENT JOURNAL TRANSMISSION             197 thru   201                          
        DSH_FILSEQNUM             ,D4           ; DOCUMENT JOURNAL FILE SEQUENCE            202 thru   205                          
        DSH_HCNTRLNUM             ,A14          ; ENVELOPE HEADER CONTROL NUMBER            206 thru   219                          
        DSH_GCNTRLNUM             ,A14          ; ENVELOPE GROUP CONTROL NUMBER             220 thru   233                          
        DSH_DCNTRLNUM             ,A14          ; ENVELOPE DOCUMENT CONTROL NUM             234 thru   247                          
        DSH_GROUP_CD              ,A5           ; GROUP CODE                                248 thru   252                          
        DSH_STOCK_LOC             ,A4           ; STOCK LOCATION                            253 thru   256                          
        DSH_CTN_ID                ,A8           ; CONTAINER ID                              257 thru   264                          
        DSH_CONTACT_CD            ,A2           ; CONTACT FUNCTION CODE                     265 thru   266                          
        DSH_CONT_NAME             ,A35          ; CONTACT NAME                              267 thru   301                          
        DSH_COMM_QL               ,A2           ; COMMUNCATIION NUMBER QUALIFIER            302 thru   303                          
        DSH_COMM_NO               ,A35          ; COMMUNICATION NUMBER                      304 thru   338                          
        DSH_FILLER                ,A82          ; FILLER                                    339 thru   420                          
                                                                                                                                    
                                                                                                                                    
RECORD  ,X                                                                                                                          
        DS830_HDR_KEY             ,A98          ; PRIMARY SORT KEY                                                                  
                                  ,A321         ; PRIMARY KEY POSITIONING                                                           
.IFDEF XX830_HDR_WORK
RECORD DS830_HDR_WORK
;
;	DIAMOND STAR Release Header - Split Processing (Temporary Work File)
;
;	Fields defined in this record are identical to TO830_HDR with the 
;	exception of the additional fields in this record of DSH_COMP_W,
;	DSH_SEQN_W and DSH_PCNT_W.
;
;	STRUCTURE:	Sequential
;	LENGTH:		436
;	SORT KEY:	DSH_COMP_W and DSH_SEQN_W
;
	DSH_COMP_W	,A8	; Company Code
	DSH_SEQN_W	,D5	; Sequence Number
	DSH_SUPP_ID_W	,A15	; Supplier ID
	DSH_RELNO_W	,A6	; Release Number
	DSH_RELDAT_W	,D8	; Release Date
	DSH_PARTNO_W	,A30	; Diamond Star Part Number
				; Pos 1-16=Pt#,Pos 17=Eng Change
	DSH_DEST_ID_W	,A15	; Destination - Ship to
	DSH_ISSUE_BY_W	,A6	; Release Issued by (division)
	DSH_DOCK_W	,A4	; Dock Code	
	DSH_LINE_FEED_W	,A2	; Factory Line Feed
	DSH_PO_NO_W	,A10	; Diamond Star Purchase Order #
				; Pos 1-2 = 'P '
				; Pos 3 = Last Digit of Year
				; Pos 4-8  = Unique Number
				; Pos 9-10 = Amend Level/Status
	DSH_PRICE_QUOTE_W,A1	; '1'=Production
				; '2'=Service
	DSH_PURPOSE_W	,A2	; Release Purpose
				; '00' = Original Release
				;
	DSH_DELIV_CODE_W,A2	; Delivery Code 
				; DL = Delivery Date
				; SD = Ship Date
	DSH_FROM_DATE_W	,D8	; Horizon From Date
	DSH_TO_DATE_W	,D8	; Horizon To Date
	DSH_QTY_IND_W	,A1	; Quantity indicator
				; A= Actual- net qty
	DSH_PART_DESCR_W,A30	; Part Description
	DSH_PKG_CODE_W	,A5	; Packaging Container Code
	DSH_PKG_QTY_W	,D6	; Quantity Per Package
	DSH_UOFM_W	,A2	; Unit Of Measure
	DSH_EXPEDITOR_W	,A2	; Contact Number/Code
	DSH_DEL_PATTERN_W,A2	; Shipment Delivery Pattern 
				; Z = Mutually Defined
	DSH_DEL_TIME_W	,A1	; Shipment Delivery Time
				; Z = Mutually Defined
	DSH_CANCEL_REF_W,A30	; Cancellation Reference Number
	DSH_TRANSNUN_W  ,A5	; DOCUMENT JOURNAL TRANSNUM
	DSH_FILSEQNUM_W ,D4     ; DOCUMENT JOURNAL FILE SEQ NUM
	DSH_HCNTRLNUM_W	,A14    ; ENVELOPE HEADER CONTROL NUMBER
	DSH_GCNTRLNUM_W	,A14    ; ENVELOPE GROUP CONTROL NUMBER
	DSH_DCNTRLNUM_W	,A14    ; ENVELOPE DOCUMENT CONTROL NUMBER
        DSH_GROUP_CD_W  ,A5     ; GROUP CODE
        DSH_STOCK_LOC_W ,A4     ; STOCK LOCATION
	DSH_CTN_ID_W    ,A8     ; CONTAINER ID 
        DSH_CONTACT_CD_W,A2     ; CONTACT FUNCTION CODE
        DSH_CONT_NAME_W ,A35    ; CONTACT NAME
        DSH_COMM_QL_W   ,A2     ; COMMUNCATIION NUMBER QUALIFIER
        DSH_COMM_NO_W   ,A35    ; COMMUNICATION NUMBER
			,A82	; Filler
	DSH_PCNT_W	,D3.2	; Percentage Split

RECORD, X
			,A8	; Company Code
			,D5	; Sequence Number
	DS830U_HDR	,A%SIZE(DS830_HDR)	;unsplit header record
.ENDC


.IFDEF XX830_HDR_TOTALS
RECORD DS830_HDR_TOTALS
;
;	Diamond Star Release Header - Split Processing. 
;	Working Storage Buffer of Percentage Fields.
;
;	The quantity fields accumulate the total quantities that have been
;	split to multiple companies.  This buffer is not written to any file.
;
	DSH_PCNT_T	,D3.2	; Percent of total split
.ENDC
