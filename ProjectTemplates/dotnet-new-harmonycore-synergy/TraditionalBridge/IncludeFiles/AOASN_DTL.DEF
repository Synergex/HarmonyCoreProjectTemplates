; AOASN_DTL.DEF
;
;	A.O. SMITH CTX Variable Length (X12) Advance Shipping Notice (ASN) 
;	Detail File.
;
;	File Organization: Isam 		Record Size: 640
;
;-------------------------------------------------------------------------------
;
;	MODIFIED	PROGRAMMER	DESCRIPTION
;	--------	----------	-----------
;	13-MAY-92	T. Morbach	Original
;	01-Dec-93	T. Bunker	Expand part numbers to 30.
;					Expand record from 512 to 640.
;-------------------------------------------------------------------------------
;
RECORD AOASN_DTL
	AOD_SHIP_ID	,D9	; Shipment Id / ASN Number.
	AOD_DEST_ABBRV	,A5	; Destination Abbreviation
	AOD_SHIP_TO	,A5	; Ship To Abbreviation
	AOD_OUR_PART	,A30	; CARaS Part Number
	AOD_CUST_PART	,A30	; Chrysler Part Number

	AOD_PO_NUM	,A13	; Chrysler Purchase Order Number

	AOD_SHIP_QTY	,D8	; Quantity Shipped
	AOD_YTD_CUM	,D8	; YTD Ship Qty
	AOD_UOFM	,A2	; Unit of Measure

	AOD_PART_QUAL	,A2	; Part Number Qualifier
				; ---------------------
				; "BP" = Part Number.
				; "RC" = Returnable Container Number.

	AOD_DOCK_CODE	,A10	; Dock Code

	AOD_BILL_NO	,A9	; Bill of Lading Number
	AOD_PACK_SLIP	,A9	; Packing Slip/Shipper Number
	
	AOD_ENG_CHG_LVL	,A3	; Engineerong Change Level
	AOD_CONTRACT	,A10	; Parts & Service Contract Number
				;
				; The Quantity Ordered and the Unit of Measure
				; Ordered are only applicable if the ASN is for
				; a Parts/Service Ship Direct Shipment.
				;
	AOD_ORDER_QTY	,D8	; Qty Ordered (Parts/Service - Ship Direct Only)
	AOD_ORDER_UOFM	,A2	; Unit of Measure Ordered (
	AOD_RET_CONT	,13A22	; Returnable Container Number
			,A191	; Filler

RECORD AOD_KEY0, X		
	;
	; Primary Key (Key 0)
	;
	K0_AOD_SHIP_ID		,D%SIZE(AOD_SHIP_ID)
	K0_AOD_DEST_ABBRV	,A%SIZE(AOD_DEST_ABBRV)
	K0_AOD_SHIP_TO		,A%SIZE(AOD_SHIP_TO)
	K0_SOD_OUR_PART		,A%SIZE(AOD_OUR_PART)

RECORD AOD_KEY1
	;
	; Alternate Segmented Key to be used in the ASN Build when we are
	; creating the Generator Input Files.
	;
	K1_AOD_SHIP_ID		,D%SIZE(AOD_SHIP_ID)
	K1_AOD_PART_QUAL	,A%SIZE(AOD_PART_QUAL)
	K1_AOD_SHIP_TO		,A%SIZE(AOD_SHIP_TO)
	K1_AOD_DEST_ABBRV	,A%SIZE(AOD_DEST_ABBRV)
	K1_AOD_CUST_PART	,A%SIZE(AOD_CUST_PART)
;
;
