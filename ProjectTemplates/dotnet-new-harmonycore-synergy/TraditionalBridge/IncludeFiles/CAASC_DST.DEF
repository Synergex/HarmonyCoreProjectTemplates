	ASCD_SID	,A30	; Original Shipment Id Number	      | 003-032
	ASCD_SID_SEQ	,D2	; Our Sequence No for dups/changes,etc| 033-034
	ASCD_SUPP_IDCD	,A17	; Original Supplier ID code	      | 035-051
	ASCD_SHFR_IDCD	,A17	; Original Supplier Ship From Id Code | 052-068
	ASCD_SHTO_IDCD	,A17	; Ship To ID code                     | 069-085
	ASCD_DEST_IDCD	,A17	; Final Dest ID code                  | 086-102
				;				      |
	ASCD_SHTO_IDQL	,A2	; Ship To ID Code Qualifier	      | 103-104
	ASCD_DEST_IDQL	,A2	; Ulitmate Destination ID Code Ql     | 105-106
				;				      |
	ASCD_BILL_NO	,A30	; Bill of Lading Number		      | 107-136
	ASCD_PACK_SLIP	,A30	; Packing Slip #		      | 137-166
	ASCD_GROSS_WGT	,D8	; Gross Weight			      | 167-174
	ASCD_WGT_UOM	,A2	; Weight Unit of Meas (LB,KK)	      | 175-176
	ASCD_SHIP_ID	,D9	; ASC SHIPMENT ID #		      | 177-185
			,A199	; Filler			      | 186-384
RECORD, X
	ASCD_KEY0 	,A102	;

RECORD	ASCD_KEY1
	K1_ASCD_SHIP_ID		,D%SIZE(ASCD_SHIP_ID)
	K1_ASCD_TP_DESIG	,A%SIZE(ASCD_TP_DESIG)
	K1_ASCD_SID		,A%SIZE(ASCD_SID)
	K1_ASCD_SID_SEQ		,D%SIZE(ASCD_SID_SEQ)
	K1_ASCD_SUPP_IDCD	,A%SIZE(ASCD_SUPP_IDCD)
	K1_ASCD_SHFR_IDCD	,A%SIZE(ASCD_SHFR_IDCD)
	K1_ASCD_SHTO_IDCD	,A%SIZE(ASCD_SHTO_IDCD)
	K1_ASCD_DEST_IDCD	,A%SIZE(ASCD_DEST_IDCD)
;
