;CARI_EMS.DEF
;
; 	CARaS/EMS Release Interface Header
;
; 	STRUCTURE: Sequential 	
;-------------------------------------------------------------------------------
;	MODIFIED	PROGRAMMER	DESCRIPTION
;	--------	----------	-----------
;	07-Sept-89	Rich Mae	Original
;	
;	30-Jan-91	C. Neely	Modifications	
;
;	19-May-92	R. Greene	Expanded AIA33
;					from 2 characters
;					to 5.
;
;	02-Dec-93	T. Bunker	Expand part numbers to 30.
;					Record sizes from 415 to 419 and
;					442 to 446.
;-------------------------------------------------------------------------------
;              ------- Record Definition -------
;-------------------------------------------------------------------------------
;
; RECORD LENGTH = 419 
;
RECORD CARI_EMS_HDR
			,A10	; Filler
	AIA1		,A30	; Customer/Trading Partners Part Number 	; RIH_TP_PART
	AIA2		,A10	; Destination Abbreviation 			; RIH_DEST_ABBRV
	AIA3		,A10	; Purchase Order Number				; RIH_PO
	AIA4		,A2	; Model Year					; RIH_MY
	AIA5		,A2	; Record Type "01"
			,A24	; Filler
	AIA8		,A4	; Constant Code - TP Designator
			,A2	; Filler
	AIA10		,A10    ; Division Abbrv				; RIH_DIV_ABBRV
	AIA11		,A6	; Daily Release Number 				; RIH_CUR_RECNO
	AIA12		,A6	; Daily Release Date				; RIH_CUR_RELDT
			,A25	; Filler
	AIA15		,D9	; Past Due Quantity				; OUR YTD PREVIOUS CUM (IF PREV_CUM.GT.0)
	AIA16		,D9	; Fab Qty/Cum - detail type "40"		
	AIA17		,D9	; Fab Qty/Cum - detail type "50"
			,A11	; Filler
	AIA20		,A2	; Unit Of Measure
			,A9	; Filler
	AIA22		,A6	; CUM Start Date				; RIH_CUM_START
	AIA23		,D9	; TP's YTD Cum Received				; RIH_BASE_CUM
	AIA24		,D9	; TP's Last Ship Quantity			; RIH_LAST_SHQT
	AIA25		,A6	; TP's Last Ship Date				; RIH_LAST_SHDT
	AIA26		,D9	; Our YTD Cum Shipped				; RIH_OUR_YTD_CUM
			,A9	; Filler 
	AIA28		,A6	; Our Last Ship Date				; RIH_OUR_LSTSHDT
	AIA29		,D9	; Our Last Ship Quantity			; RIH_OUR_LSTQTY
			,A27	; Filler
	AIA33		,A5	; Dock Code					; RID_DOCK
			,A62	; Filler
	AIA43		,A10	; Sales Order #
			,A62	; Filler

;
; RECORD LENGTH = 446
;
RECORD CARI_EMS_DTL
			,A10	; Filler
	AIB1		,A30	; Customer/Trading Partner Part #
	AIB2		,A10	; Ship To/Destination Abbrv
	AIB3		,A10  	; Purchase Order Number
	AIB4		,A2	; Model Year
	AIB5		,A2	; Record Type, "02" = Detail Record
	AIB6		,D2	; 	"1" = Hourly
				;  	"2" = Daily
				;	"3" = Weekly
				;       "4" = Monthly
				;       "5" = User Defined ; Forecast	
	AIB7		,D2	; Record Number

	AIB99		,12A28	; Requirement String Array
				; ------------------------
				; String(1,6)   = Required Date
				; String(7,10)  = Required Time
				; String(11,19) = Required Quantity
				; String(20,28) = Required Cum YTD
				;
			,A12	; Filler
	AIB15		,A4	; Weekly Ship Code
			,A26	; Filler

RECORD	CARI_EMS_DATA	
			,A%SIZE(CARI_EMS_DTL)
RECORD	,X
			,A62
	RECORD_TYPE	,A2

