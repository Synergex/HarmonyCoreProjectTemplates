RECORD GM830_HDR
;
; GM830_HDR.DEF - GM MATERIAL RELEASE HEADER
;
; 	STRUCTURE: 	SEQUENTIAL
; 	RECORD LEN: 	290
;
;	08-May-92	Bill Schmelter	Modified record to include 11 buckets
;					to hold forecast qty's/dates (from 8).
;	19-Nov-96	B. Sparks	Added field to store delivery location
;-------------------------------------------------------------------------------
;              ------- Record Definition -------
;-------------------------------------------------------------------------------
	MRCTLS		,D5		; CONTROLLING SOURCE
	MRRELN		,A3		; RELEASE NUMBER
	MRPART		,A8		; CUSTOMER PART NUMBER
	MRMY		,A1		; MODEL YEAR
	MRPONO		,A9		; P.O. NUMBER
	MRRELD		,D8		; RELEASE DATE (MM/DD/Y)
	MRFABQ		,D9		; AUTHORIZED FAB QTY
	MRFBNC		,D7		; FAB NET CHANGE
	MRMATQ		,D9		; AUTHORIZED MATERIAL QTY
	MRMTNC		,D7		; MATERIAL NET CHANGE
	MRTRAF		,A4		; TRAFFIC COMMODITY CODE
	MRCONC		,A4		; CONTROL CODE
	MRSPEC		,D9		; SPECIAL AUTHORIZATION
	MRPLAN		,D9		; PLANNING QTY
	MRBOMY		,D9		; BUILD OUT MODEL YEAR REQUIREMENTS
	MRYTD		,D9		; HIGHEST YEAR-TO-DATE CUM
	MRYTDC		,D7		; CHANGE IN CUM FROM PREVIOUS RELEASE
	MRSAMP		,A1		; SAMPLE CODE (BLANK IF UNUSED)
	MRCLAS		,A1		; CLASSIFICATION CODE
	MRFINL		,A1		; FINAL CODE
	MRPACK		,A1		; PACKAGE=P, NOT PACKAGED=N or ' '
 	MRPLNQ		,11D7		; FUTURE PLAN WEEK QUANTITIES
	MRPLNW		,11D8		; FUTURE PLAN WEEK DATES
	MRWKDT		,4D8		; WEEKLY QUANTITY DATES
	MRSUPP		,A9		; SUPPLIER ID
	MRDELO		,A8		; DELIVERY LOCATION

RECORD	,X
	KEYMR		,A17		; KEY (ctl source,rel,part,my)

.IFDEF XX830_HDR_WORK
RECORD GM830_HDR_WORK
;
; 			GM MATERIAL RELEASE HEADER - SPLIT PROCESSING 
;			TEMPORARY WORK FILE
;
;			Field defined in the record are identical to 
;			to GM830_HDR with the exception of the additional
;			fields in this record of MRCOMP_W, MRSEQN_W,
;			and MRPCNT_W
;
; 	STRUCTURE: 	SEQUENTIAL
; 	RECORD LEN: 	266
;	SORT KEY:	MRCOMP_W, MRSEQN_W
;
;-------------------------------------------------------------------------------
;              ------- Record Definition -------
;-------------------------------------------------------------------------------
	MRCOMP_W	,A8		; COMPANY CODE
	MRSEQN_W	,D5		; SEQUENCE NUMBER
	MRCTLS_W		,D5		; CONTROLLING SOURCE
	MRRELN_W		,A3		; RELEASE NUMBER
	MRPART_W		,A8		; CUSTOMER PART NUMBER
	MRMY_W			,A1		; MODEL YEAR
	MRPONO_W		,A9		; P.O. NUMBER
	MRRELD_W		,D8		; RELEASE DATE (MM/DD/Y)
	MRFABQ_W		,D9		; AUTHORIZED FAB QTY
	MRFBNC_W		,D7		; FAB NET CHANGE
	MRMATQ_W		,D9		; AUTHORIZED MATERIAL QTY
	MRMTNC_W		,D7		; MATERIAL NET CHANGE
	MRTRAF_W		,A4		; TRAFFIC COMMODITY CODE
	MRCONC_W		,A4		; CONTROL CODE
	MRSPEC_W		,D9		; SPECIAL AUTHORIZATION
	MRPLAN_W		,D9		; PLANNING QTY
	MRBOMY_W		,D9		; BUILD OUT MODEL YEAR REQUIREMENTS
	MRYTD_W			,D9		; HIGHEST YEAR-TO-DATE CUM
	MRYTDC_W		,D7		; CHANGE IN CUM FROM PREVIOUS RELEASE
	MRSAMP_W		,A1		; SAMPLE CODE (BLANK IF UNUSED)
	MRCLAS_W		,A1		; CLASSIFICATION CODE
	MRFINL_W		,A1		; FINAL CODE
	MRPACK_W		,A1		; PACKAGE=P, NOT PACKAGED=N or ' '
 	MRPLNQ_W		,11D7		; FUTURE PLAN WEEK QUANTITIES
	MRPLNW_W		,11D8		; FUTURE PLAN WEEK DATES
	MRWKDT_W		,4D8		; WEEKLY QUANTITY DATES
	MRSUPP_W		,A9		; SUPPLIER ID
	MRDELO_W		,A8		; DELIVERY LOCATION
	MRPCNT_W		,D3.2		; PERCENT OF TOTAL SPLIT
RECORD	,X
			,A8		; COMPANY CODE
			,D5		; SEQUENCE NUMBER
	GM830U_HDR	,A%SIZE(GM830_HDR) ; UNSPLIT HEADER
	
.ENDC

.IFDEF XX830_HDR_TOTALS
RECORD GM830_HDR_TOTALS
;
;			GM MATERIAL RELEASE HEADER - SPLIT PROCESSING 
;			WORKING STORAGE BUFFER OF QUANTITY FIELDS.
;			
;			The quantity fields accumlate the total quantities
;			that have been split to the multiple companies.
;			This buffer is not written to any file.
;
	MRFABQ_T	,D9		; AUTHORIZED FAB QTY
	MRFBNC_T	,D7		; FAB NET CHANGE
	MRMATQ_T	,D9		; AUTHORIZED MATERIAL QTY
	MRMTNC_T	,D7		; MATERIAL NET CHANGE
	MRSPEC_T	,D9		; SPECIAL AUTHORIZATION
	MRPLAN_T	,D9		; PLANNING QTY
	MRBOMY_T	,D9		; BUILD OUT MODEL YEAR REQUIREMENTS
	MRYTD_T		,D9		; HIGHEST YEAR-TO-DATE CUM
	MRYTDC_T	,D7		; CHANGE IN CUM FROM PREVIOUS RELEASE
	MRPLNQ_T	,11D7		; FUTURE PLAN WEEK QUANTITIES
	MRPCNT_T	,D3.2		; PERCENT OF TOTAL SPLIT
.ENDC
