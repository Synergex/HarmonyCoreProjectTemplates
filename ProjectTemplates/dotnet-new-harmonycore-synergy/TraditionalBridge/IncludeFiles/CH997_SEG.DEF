; CH997_SEG.DEF
;
;	Functional Acknowledgment Detail - Segment Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	09-Jul-89	J. Rheingans	Original
;
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD CH997_SEG
	CFS_GROUP_ID	,A2	; Functional Group Identifier	|   1 thru 2
	CFS_DATA_INT	,D9	; Data Interchange Control #	|   3 thru 11
	CFS_TRANS_TYPE	,A3	; Transaction Set Identifier	|  12 thru 14
	CFS_TRANS_CTL	,D9	; Trasnaction Set Control #	|  15 thru 23
	CFS_SEGMENT	,A3	; Segment Identifier		|  24 thru 26
	CFS_SEG_POS	,D6	; Segment Position		|  27 thru 32
	CFS_LOOP_ID	,D4	; Loop Identifier		|  33 thru 36

	CFS_NOTE_1	,D3	; Segment Note Code-1		|  37 thru 39
	CFS_NOTE_2	,D3	; Segment Note Code-2		|  40 thru 42
	CFS_NOTE_3	,D3	; Segment Note Code-3		|  43 thru 45
	CFS_NOTE_4	,D3	; Segment Note Code-4		|  46 thru 48
	CFS_NOTE_5	,D3	; Segment Note Code-5		|  49 thru 51
			,A49	; Filler			|  52 thru 100

RECORD CFS_KEY, X
	K_CFS_GROUP_ID		,A%SIZE(CFS_GROUP_ID)
	K_CFS_DATA_INT		,D%SIZE(CFS_DATA_INT)
	K_CFS_TRANS_TYPE	,A%SIZE(CFS_TRANS_TYPE)
	K_CFS_TRANS_CTL		,D%SIZE(CFS_TRANS_CTL)
	K_CFS_SEGMENT		,A%SIZE(CFS_SEGMENT)
	K_CFS_SEG_POS		,D%SIZE(CFS_SEG_POS)
	K_CFS_LOOP_ID		,D%SIZE(CFS_LOOP_ID)

