;CAEINV_SNO.DEF
;
;	Serial No File for 
;       DATE : 28-OCT-92
;       File Organization : INDEXED
;       Record Size :   270                               Field Positions
;	
;	22-Sep-93	T. Bunker	Added EIS_COMP_CODE,EIS_IMPEXP_FLG, and
;					alternate buffer.
;	01-Dec-93	T. Bunker	Increase part number to 30.  Increase
;					record size 270.
;-------------------------------------------------------------------------------
RECORD  CAEINV_SNO
	EIS_TP_DESIG	,A2	; Trading Partner Designator
	EIS_INV_NO	,A9	; Invoice No
        EIS_CONT_ID     ,A25    ; Master Container Marks/Numbers
				;  -  Not currently used, always blank
        EIS_OUR_PART    ,A30    ; Customer Part Number
        EIS_SHIP_REF    ,A20    ; Ship Reference Number
        EIS_MARKS_NUM   ,A25    ; Marks and Numbers (MAN02)(serial no)
				;
	EIS_SHIPPER	,D9	; Shipper Number
	EIS_COMP_CODE	,A8	; Company code
	EIS_IMPEXP_FLG	,D1	; Import/export Flag
				; 0 - import
				; 1 - export
			,A141	; Filler
				;

	;Key 0 no duplicates, no changes
RECORD  ,X
	EIS_KEY0        ,A111   ; PRIMARY KEY POSITIONING
				;

	;Key 1 no duplicates, no changes
RECORD	EIS_KEY1
	K1_EIS_TP_DESIG	,A%SIZE(EIS_TP_DESIG)
	K1_EIS_MARKS_NUM,A%SIZE(EIS_MARKS_NUM)

; ----------------------------------------------------------------------------- 
; Alternate Record Definition ------------------------------------------------- 
; ----------------------------------------------------------------------------- 

.IFDEF CAEINV_SNO_ALT

RECORD  A_CAEINV_SNO
	A_EIS_TP_DESIG	  ,A2	  ; Trading Partner Designator
	A_EIS_INV_NO	  ,A9	  ; Invoice No
        A_EIS_CONT_ID     ,A25    ; Master Container Marks/Numbers
				  ;  -  Not currently used, always blank
        A_EIS_OUR_PART    ,A30    ; Customer Part Number
        A_EIS_SHIP_REF    ,A20    ; Ship Reference Number
        A_EIS_MARKS_NUM   ,A25    ; Marks and Numbers (MAN02)(serial no)
				  ;
	A_EIS_SHIPPER	  ,D9	  ; Shipper Number
	A_EIS_COMP_CODE	  ,A8	  ; Company code
	A_EIS_IMPEXP_FLG  ,D1	  ; Import/export Flag
				  ; 0 - import
				  ; 1 - export
			  ,A141	  ; Filler
				  ;

	;Key 0 no duplicates, no changes
RECORD  ,X
	A_EIS_KEY0        ,A111   ; PRIMARY KEY POSITIONING
				;

	;Key 1 no duplicates, no changes
RECORD	A_EIS_KEY1
	A_K1_EIS_TP_DESIG	,A%SIZE(EIS_TP_DESIG)
	A_K1_EIS_MARKS_NUM,A%SIZE(EIS_MARKS_NUM)
.ENDC
