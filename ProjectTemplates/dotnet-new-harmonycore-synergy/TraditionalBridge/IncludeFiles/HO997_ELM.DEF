; HO997_ELM.DEF
;
;	Functional Acknowledgment Detail - Element Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	29-Oct-90	J. Curl		Original
;
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 160		| Field Position
;-------------------------------------------------------------------------------

RECORD HO997_ELM
	HFE_GROUP_ID	,A2	; Functional Group Identifier	|   1 thru 2
	HFE_DATA_INT	,D9	; Data Interchange Control #	|   3 thru 11
	HFE_TRANS_TYPE	,A3	; Transaction Set Identifier	|  12 thru 14
	HFE_TRANS_CTL	,D9	; Trasnaction Set Control #	|  15 thru 23
	HFE_SEGMENT	,A3	; Segment Identifier		|  24 thru 26
	HFE_SEG_POS	,D6	; Segment Position in Trans Set	|  27 thru 32
	HFE_LOOP_ID	,D4	; Loop Identifier		|  33 thru 36
	HFE_ELEM_POS	,D2	; Element Position in Segment	|  37 thru 38

	HFE_ELEM_REF	,D4	; Element Reference # 		|  39 thru 42
	HFE_NOTE_CODE	,D3	; Element Note Code		|  43 thru 45
	HFE_ELEM_COPY	,A80	; Copy of Bad Element		|  46 thru 125
			,A35	; Filler			| 126 thru 160

RECORD HFE_KEY, X
	K_HFE_GROUP_ID		,A%SIZE(HFE_GROUP_ID)
	K_HFE_DATA_INT		,D%SIZE(HFE_DATA_INT)
	K_HFE_TRANS_TYPE	,A%SIZE(HFE_TRANS_TYPE)
	K_HFE_TRANS_CTL		,D%SIZE(HFE_TRANS_CTL)
	K_HFE_SEGMENT		,A%SIZE(HFE_SEGMENT)
	K_HFE_SEG_POS		,D%SIZE(HFE_SEG_POS)
	K_HFE_LOOP_ID		,D%SIZE(HFE_LOOP_ID)
	K_HFE_ELEM_POS		,D%SIZE(HFE_ELEM_POS)
