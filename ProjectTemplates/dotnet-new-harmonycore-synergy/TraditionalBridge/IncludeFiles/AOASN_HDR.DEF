	A_AOH_BILL_TO		,A5	; Bill To Abbreviation
	A_AOH_SUPPL_ABBRV	,A5	; Supplier Abbreviation
	A_AOH_SHIP_FROM		,A5	; Ship From Abbreviation 
	A_AOH_ASN_DATE		,D6	; ASN Creation Date, Format: YYMMDD
	A_AOH_ASN_TIME		,D4	; ASN Creation TIme, Format: HHMM
	A_AOH_ASN_PURPOSE	,A2	; ASN Purpose
	A_AOH_SHIP_DATE		,D6	; Ship Date, Format: YYMMDD
	A_AOH_SHIP_TIME		,D4	; Ship Time, Format: HHMM
	A_AOH_TIME_ZONE		,A2	; Suppliers Local Time Zone Code
	A_AOH_NET_WGT		,D8	; Net Weight,   Format: XX,XXX,XXX
	A_AOH_GROSS_WGT		,D8	; Gross Weight, Format: XX,XXX,XXX
	A_AOH_WEIGHT_TYPE	,A2	; Weight Type: LB=Pounds, KG=Kilograms
	A_AOH_CARRIER		,A4	; Carrier SCAC Code
	A_AOH_ROUTE_MODE	,A2	; Transportation/Routing Mode (X12 Table # 91)
	A_AOH_POOL_LOC		,A7	; Pool Location Location
	A_AOH_EQUIP_CODE	,A2	; Shipping Equipment Code (X12 Table # 40)
	A_AOH_EQUIP_PRE		,A4	; Shipping Equipment Prefix
	A_AOH_EQUIP_NO		,A7	; Equipment Number
	A_AOH_CONT_CODE		,A5	; Shipping Container Code (Table #: 103)
	A_AOH_CONT_QTY		,D7	; Number of Shipping Containers
	A_AOH_BILL_NO		,A9	; Bill of Lading Number
	A_AOH_PACK_SLIP		,A9	; Packing Slip/Shipper Number
	A_AOH_MAST_BL		,A9	; Master Bill of Lading Number
	A_AOH_FREIGHT_NO	,A16	; Freight Bill Number
	A_AOH_AIR_BILL		,A16	; Air Bill Number
	A_AOH_AETC		,A8	; A.E.T.C. Number
	A_AOH_RESPON_CODE	,A1	; A.E.T.C. Responsibility Code
	A_AOH_SEAL_ARRAY	,20A15	; Array for Seal Numbers
	A_AOH_TRANS_DATE	,D6	; Date Transmitted, Format: YYMMDD
	A_AOH_TRANS_TIME	,D4	; Time Transmitted, Format: HHMM
	A_AOH_TRANS_CTL		,D9	; Transaction Set Control Number
	A_AOH_ASN_NO		,A11	; ASN No
	A_AOH_SHIP_SEQ_NO	,D15	; Shipment sequence number
	A_AOH_TYPE        	,A2	; Shipment sequence number
				,A71	; Filler
.ENDC
