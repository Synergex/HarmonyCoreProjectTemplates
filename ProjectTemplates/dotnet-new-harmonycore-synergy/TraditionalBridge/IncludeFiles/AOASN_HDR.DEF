; AOASN_HDR.DEF
;
;	A.O. SMITH Advance Shipping Notice (ASN) Header File.
;
;	File Organization: Isam 		Record Size: 512
;
;	Primary Key --> AOH_SHIP_ID (Positions 1 thru 9)
;
;	*Note: If AOH_SHIP_ID.EQ.000000000
;		Then	Allow Duplicates (A Default Record)
;
;		Else	Do Not Allow Duplicate Ship Ids.
;
;-------------------------------------------------------------------------------
;
;	MODIFIED	PROGRAMMER	DESCRIPTION
;	--------	----------	-----------
;	13-MAY-92	T. Morbach	Original
;	22-MAY-97	C. Leonatti	Added TYPE - CC or PP
;
;-------------------------------------------------------------------------------
;
;
RECORD AOASN_HDR
	AOH_SHIP_ID	,D9	; Shipment Id Number (Primary Key).
	AOH_DEST_ABBRV	,A5	; Destination Abbreviation
	AOH_SHIP_TO	,A5	; Ship To Abbreviation
	AOH_BILL_TO	,A5	; Bill To Abbreviation

	AOH_SUPPL_ABBRV	,A5	; Supplier Abbreviation
	AOH_SHIP_FROM	,A5	; Ship From Abbreviation 

	AOH_ASN_DATE	,D6	; ASN Creation Date, Format: YYMMDD
	AOH_ASN_TIME	,D4	; ASN Creation TIme, Format: HHMM

	AOH_ASN_PURPOSE	,A2	; ASN Purpose
				; -----------
				; "00" = Original
				; "01" = Cancel
				; "19" = Correct YTD Cum

	AOH_SHIP_DATE	,D6	; Ship Date, Format: YYMMDD
	AOH_SHIP_TIME	,D4	; Ship Time, Format: HHMM
	AOH_TIME_ZONE	,A2	; Suppliers Local Time Zone Code

	AOH_NET_WGT	,D8	; Net Weight,   Format: XX,XXX,XXX
	AOH_GROSS_WGT	,D8	; Gross Weight, Format: XX,XXX,XXX
	AOH_WEIGHT_TYPE	,A2	; Weight Type Qualifier LB=Pounds, KG=Kilograms
	AOH_CARRIER	,A4	; Carrier SCAC Code
	AOH_ROUTE_MODE	,A2	; Transportation/Routing Mode (X12 Table # 91)
	AOH_POOL_LOC	,A7	; Pool Location Location

	AOH_EQUIP_CODE	,A2	; Shipping Equipment Code (X12 Table # 40)
	AOH_EQUIP_PRE	,A4	; Shipping Equipment Prefix
	AOH_EQUIP_NO	,A7	; Equipment Number

	AOH_CONT_CODE	,A5	; Shipping Container Code (Table #: 103)
	AOH_CONT_QTY	,D7	; Number of Shipping Containers

	AOH_BILL_NO	,A9	; Bill of Lading Number
	AOH_PACK_SLIP	,A9	; Packing Slip/Shipper Number
	AOH_MAST_BL	,A9	; Master Bill of Lading Number
	AOH_FREIGHT_NO	,A16	; Freight Bill Number
	AOH_AIR_BILL	,A16	; Air Bill Number

	AOH_AETC	,A8	; A.E.T.C. Number
	AOH_RESPON_CODE	,A1	; A.E.T.C. Responsibility Code

	AOH_SEAL_ARRAY	,20A15	; Array for Seal Numbers

	AOH_TRANS_DATE	,D6	; Date Transmitted, Format: YYMMDD
	AOH_TRANS_TIME	,D4	; Time Transmitted, Format: HHMM
	AOH_TRANS_CTL	,D9	; Transmitted with Transaction Set Control #
	AOH_ASN_NO	,A11	; ASN Number
	AOH_SHIP_SEQ_NO	,D15	; Shipment sequence number
	AOH_TYPE	,A2	; (CC = Collect PP = Pre Paid)
			,a71	; Filler

;-------------------------------------------------------------------------------
;	-------- Alternate Key Definitions --------
;-------------------------------------------------------------------------------

RECORD AOH_KEY1
	;
	; Used in the ASN Maintenance and Load to Retrieve the ASN Default 
	; Records.  ASN/Shipper Number is set to zeros.
	;
	; No Duplicates, No Changes Allowed.
	;
	K1_AOH_SHIP_ID		,D%SIZE(AOH_SHIP_ID)
	K1_AOH_DEST_ABBRV	,A%SIZE(AOH_DEST_ABBRV)

RECORD AOH_KEY2
	;
	; Used when building the Generator Input Files, to group ASNs together
	; by Supplier Abbrv, Ship From Abbrv, Master Bill Number, and ASN #.
	;
	; No Duplicates Allowed, Changes are Allowed.
	;
	K2_AOH_SUPPL_ABBRV	,A%SIZE(AOH_SUPPL_ABBRV)
	K2_AOH_SHIP_FROM	,A%SIZE(AOH_SHIP_FROM)
	K2_AOH_MAST_BL		,A%SIZE(AOH_MAST_BL)
	K2_AOH_SHIP_ID		,D%SIZE(AOH_SHIP_ID)

RECORD AOH_KEY3
	;
	; Transaction Set Control Number as an Alternate Key, used in the 824
	; Report to look up Reported ASNs.
	;
	; Duplicates and Changes are Allowed.
	;
	K3_AOH_TRANS_CTL		,D%SIZE(AOH_TRANS_CTL)

RECORD AOH_KEY4
	K4_AOH_ASN_NO			,A%SIZE(AOH_ASN_NO)

;-------------------------------------------------------------------------------
;              ------- Alternate Record Defintion -------
;-------------------------------------------------------------------------------

.IFDEF AOASN_HDR_ALT
RECORD A_AOASN_HDR
			,A%SIZE(AOASN_HDR)
RECORD,X
	A_AOH_SHIP_ID		,D9	; Shipment Id Number (Primary Key).
	A_AOH_DEST_ABBRV	,A5	; Destination Abbreviation
