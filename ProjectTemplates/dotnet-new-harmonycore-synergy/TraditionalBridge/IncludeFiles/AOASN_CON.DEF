; AOASN_CON.DEF
;
;	A.O. SMITH CTX Variable Length (X12) Advance Shipping Notice (ASN) 
;	Container File.
;
;	File Organization: Isam 		Record Size: 128
;
;-------------------------------------------------------------------------------
;
;	MODIFIED	PROGRAMMER	DESCRIPTION
;	--------	----------	-----------
;	13-MAY-92	T. Morbach	Original
;	01-Dec-93	T. Bunker	Expand part number to 30.
;					Expand record from 100 to 128.
;-------------------------------------------------------------------------------
;
RECORD AOASN_CON
	AOC_SHIP_ID	,D9	; Shipment Id / ASN Number
	AOC_DEST_ABBRV	,A5	; Destination Abbreviation
	AOC_SHIP_TO	,A5	; Ship To Abbreviation
  	AOC_OUR_PART	,A30	; CARaS Part Number       
	AOC_MAST_SNO	,A15	; Master Serial Number
	AOC_CONT_CODE	,A5	; Container Type (X12 Table # 103)
	AOC_UNITS_PER	,D7	; Number of Units per Container   
	AOC_LOADS	,D5	; Number of Loads (Containers)    
			,A47	; Filler
RECORD AOC_KEY0, X
	;
	; Primary Key, No Duplicates No Changes (Length:59, Position:0)
	;
	K0_AOC_SHIP_ID		,D%SIZE(AOC_SHIP_ID)
	K0_AOC_DEST_ABBRV	,A%SIZE(AOC_DEST_ABBRV)
	K0_AOC_SHIP_TO		,A%SIZE(AOC_SHIP_TO)
  	K0_AOC_OUR_PART		,A%SIZE(AOC_OUR_PART)
	K0_AOC_MAST_SNO		,A%SIZE(AOC_MAST_SNO)
	K0_AOC_CONT_CODE	,A%SIZE(AOC_CONT_CODE)
	K0_AOC_UNITS_PER	,A%SIZE(AOC_UNITS_PER)

RECORD AOC_KEY1
	;
	; Segmented Alternate Key, No Duplicates No Changes.
	;
	K1_AOC_SHIP_ID		,D%SIZE(AOC_SHIP_ID)
	K1_AOC_MAST_SNO		,A%SIZE(AOC_MAST_SNO)
	K1_AOC_SHIP_TO		,A%SIZE(AOC_SHIP_TO)
	K1_AOC_DEST_ABBRV	,A%SIZE(AOC_DEST_ABBRV)
  	K1_AOC_OUR_PART		,A%SIZE(AOC_OUR_PART)
	K1_AOC_CONT_CODE	,A%SIZE(AOC_CONT_CODE)
	K1_AOC_UNITS_PER	,D%SIZE(AOC_UNITS_PER)
