; FOTMPF_HDR.DEF
;
;	File Definition for Ford's Fixed Length Temporary ASN File.
;	This File is used to build the Actual Transmitted File (FOASN_RJE)
;	to Ford.
;
;	File Organization: Sequential		File Size: 80
;
RECORD FOTMPF_HDR
	TH_CONV_NO	,A10		; Conveyance Number.
	TH_DEST_CODE	,A5		; Destination Code.
	TH_SHIP_ID	,D9		; Shipment Id Number
	TH_CONV_CODE	,A1		; Conveyance Code.
	TH_BILL_LADING	,D6		; Bill of Lading Number.
	TH_CARRIER	,A12		; Carrier Code.
	TH_SHIP_DAY	,D2		; Day Shipped.
	TH_SHIP_TIME	,D4		; Time Shipped, Format: HHMM
	TH_CONTAINERS	,A6		; Containers.
	TH_SUPP_CODE	,A5		; Supplier Code.
	TH_TOTAL_WEIGHT	,D6		; Total Shipment Weight.
	TH_DIV_ABBRV	,A5		; Division Abbreviation.
	TH_DEST_ABBRV	,A5		; Destination Abbreviation.
			,A4		; Filler
RECORD, X
	HDR_KEY		,A24		; Sort Key.

RECORD LAST_HDR
			,A24		; Used to Check if Sort is Needed.

