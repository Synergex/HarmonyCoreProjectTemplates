RECORD GM862_NTE
;
; GM862_NTE.DEF	- GM DAILY MATERIAL RELEASE NOTES	(New)
;
; 	STRUCTURE: 	SEQUENTIAL
; 	RECORD LEN: 	104
;
;-------------------------------------------------------------------------------
;              ------- Record Definition -------
;-------------------------------------------------------------------------------
;
	MPRTNO		,A30		;PART NUMBER
	MDEST		,D5		;DESTINATION
	MDOCK		,A5		;DOCK CODE
	MRELNO		,D6		;RELEASE NUMBER
	MMESSG		,A58		;FREE FORM MESSAGE
RECORD	,X
	KEYDNT		,A46

RECORD	,X		; REDEFINE NUMERIC TO ALPHA FOR NON QUANTITY/DATE FIELDS
	MPRTNO_A	,A30		;PART NUMBER
	MDEST_A		,A5		;DESTINATION
	MDOCK_A		,A5		;DOCK CODE
	MRELNO_A	,A6		;RELEASE NUMBER
	MMESSG_A	,A58		;FREE FORM MESSAGE


.IFDEF XX862_NTE_WORK
RECORD GM862_NTE_WORK
;
; 			GM DAILY MATERIAL RELEASE NOTES - SPLIT PROCESSING 
;			TEMPORARY WORK FILE
;
;			Field defined in the record are identical to 
;			to GM862_NTE with the exception of the additional
;			fields in this record of NCOMP_W and NSEQN_W.
;			Since the Unsplit file is not sorted,
;			the sequnce number is used to maintain the same
;			sequence when the work file is sorted.
;
; 	STRUCTURE: 	SEQUENTIAL
; 	RECORD LEN: 	117
;	SORT KEY:	NCOMP_W, NSEQN_W
;
;-------------------------------------------------------------------------------
;              ------- Record Definition -------
;-------------------------------------------------------------------------------
;
	NCOMP_W		,A8	;COMPANY CODE
	NSEQN_W		,D5	;SEQUENCE NUMBER	
	GM862U_NTE	,A%SIZE(GM862_NTE)
.ENDC
