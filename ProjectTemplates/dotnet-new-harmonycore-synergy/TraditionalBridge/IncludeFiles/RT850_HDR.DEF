; RT850_HDR.DEF
;
;	Retail X12 Purchase Order (850) Header File.
;
;	Date		Analyst		Modification
;	-----------	---------------	--------------------------------------
;	11-Jan-1993	Bill Schmelter	Increased record size from 640 to 768
;					Added RTH_SENDER_ID_QL & RTH_CONTRACT
;					RTH_SALES_REGN, RTH_CONSOLID_PNT,
;					RTH_TRANS_SET
;	10-FEB-1993	Bill Schmelter	Added SHIP_TIME,SNB_TIME,SNA_TIME,
;					DELIV_TIME, RTH_DNA_TIME (AAFES)
;	03-DEC-1993	Bill Schmelter	Added DI_CONTACT and DI_PHONE
;					Added CURRENCY,CURRENCY_QL
;	14-Dec-1993	Bill Schmelter	Increased record size from 768 to 864
;
;	05-MAY-1994	John Sawyer III	Added PRE_SHIP field and buyer qualifier
;					to aid in the convert.
;-----------------------------------------------------------------------------
; File Organization: Sequential | Max Record Size: 768	|  Positions
;     				| Variable Length	|
;-----------------------------------------------------------------------------

RECORD RT850_HDR
		;----- Sort Key Information -------------
	RTH_SUPP_ID	,A15	; Vendor ID / Receiv ID	|   1 thru 15
	RTH_PO_NO	,A22	; Purchase order number |  16 thru 37
	RTH_PO_DATE	,D6	; Purchase order date	|  38 thru 43
	RTH_PO_TYPE	,A2	; Table 92		|  44 thru 45
				;			|
		;------PO Information	-----------------
	RTH_PURPOSE	,A2	; Transaction Purpose	|  46 thru 47
				; 00 = Original Order   |
				; 01 = Cancellation     |
				; 04 = Change           |
				; 06 = Confirming Order |
				; 07 = Duplicate Order	|
				;			|
	RTH_DEPT	,A15	; Department Code	|  48 thru 62 
	RTH_CLASS	,A2	; Class Code		|  63 thru 64
	RTH_PO_REF_NO	,A45	; Reference No		|  65 thru 109
				;			|
		;----- Contact/Buyer --------------------
	RTH_PER_DEPT	,A2	; Contact Department	| 110 thru 111
	RTH_PER_NAME	,A35	; Contact/Buyer Name	| 112 thru 146
	RTH_PER_PHONE	,A21	; Contact/Buyer Phone	| 147 thru 167
				;			|
		;------Sender/Test Information ----------
	RTH_VENDOR_ID	,A15	; Vendor Number		| 168 thru 182
	RTH_RECVR_ID	,A15	; Receivers Id/Dun No	| 183 thru 197
	RTH_TEST_IND	,A1	; Test indicator 	| 198 thru 198
				; T= Test, P=Production |
				;
		; ----- Destination ID ------------------
	RTH_SHIP_TO_DEF	,A15	; ShipTo Default(ie N1) | 199 thru 213
	RTH_BILL_TO_DEF	,A15	; BillTo Default(ie N1) | 214 thru 228
				;
		;------ Shipment/Scheduling Dates -------
	RTH_SCHED_CODE	,4A2	; Schedule Code	(SHH)	| 229 thru 236
	RTH_BACKORDER	,A1	; Y=Backorder,N=No	| 237 thru 237
				;-- Ship
	RTH_SHIP_DATE	,D6	; Requested Ship  Date	| 238 thru 243
	RTH_SNB_DATE	,D6	; Ship Not Before	| 244 thru 249
	RTH_SNA_DATE	,D6	; Shp Not Aft/Cancel Dte| 250 thru 255
				;-- Deliver  
	RTH_DELIV_DATE	,D6	; Requested Deliv Date	| 256 thru 261
	RTH_DNA_DATE	,D6	; Deliver Not After	| 262 thru 267
				;			|
		;------- Promotion Data -----------------
	RTH_PROMO_DATE	,D6	; Promotional Date	| 268 thru 273
	RTH_PROMO_EVENT	,A10	; Promotion Ref no	| 274 thru 283
	RTH_PROMO_TXT	,A80	; Promotion Identifier	| 284 thru 363
				;
		;-----TD5 - Routing Information ----------
	RTH_RTE_SEQ_CODE,A2	; Routing Sequence T#133| 364 thru 365
	RTH_RTE_MODE	,A2	; Transport Mode   T#091| 366 thru 367
	RTH_RTE_CARRIER	,A35	; Carrier Name 		| 368 thru 402
	RTH_RTE_STATUS	,A2	; PD=?Paid		| 403 thru 404
				;
		;------FOB - Free On Board Information --
	RTH_MTH_PMT	,A2	; Carrier pmt mthod #146| 405 thru 406
	RTH_TR_RESP_LOC	,A2	; Tportation Resp Locatn| 407 thru 408
				; Locat qual Table #376	|
	RTH_TRD_TERM_QL	,A2	; Trade term Qual  #334	| 409 thru 410
				; 01=Incoterms,02=TDCC	|
	RTH_TRD_TERM_CD	,A3	; Trade Term code	| 411 thru 413
				; Based on TRD_TERM_QL	|
				; 01= ICC-table unknown	|
				; 02= Use Table #335	|
	RTH_TITLE_PASS	,A2	; Title Passage Cod #377| 414 thru 415
	RTH_TR_RESP_TXT	,A80	; Tportation Resp Txt	| 416 thru 495
	RTH_FOB_TEXT	,A80	; Text re: FOB Point	| 496 thru 575
				;
	RTH_RELEASE	,A15	; Release Number	| 576 thru 590
	RTH_DISTRB_CTR	,A15	; Distribution ctr #    | 591 thru 605
				; 			|
		;-- P/O Change Information -------------|
	RTH_PO_CHANGE	,D6	; P/O Change Date	| 606 thru 611
		;--- ISA sender Id	----------------|
	RTH_SENDER_ID	,A15	; ISA sender ID		| 612 thru 626
	RTH_SENDER_ID_QL,A2	; ISA sender ID ql	| 627 thru 628
				;			|
	RTH_CONTRACT	,A15	; Contract Number	| 629 thru 643
	RTH_SALES_REGN	,A6	; Sales Admin Region id | 644 thru 649
	RTH_CONSOLID_PNT,A15	; Consolidation point	| 650 thru 664
	RTH_TRANS_SET	,A3	; Trans Set (850/860)	| 665 thru 667 
	RTH_SHIP_TIME	,D4	; Requested Ship  Time	| 668 thru 671
	RTH_SNB_TIME	,D4	; Ship Not Before Time	| 672 thru 675
	RTH_SNA_TIME	,D4	; Shp Not Aft/Cancel Tim| 676 thru 679
	RTH_DELIV_TIME	,D4	; Requested Deliv Tim	| 680 thru 683
	RTH_DNA_TIME	,D4	; Deliver Not After Tim	| 684 thru 687
	RTH_DI_CONTACT	,A35	; Delivery Instr Contact| 688 thru 722 
	RTH_DI_PHONE	,A25	; Delivery Instr Phone# | 723 thru 747
	RTH_CURRENCY_QL	,A2	; Currency Qualifier	| 748 thru 749
	RTH_CURRENCY	,A3	; Currency 		| 750 thru 752
	RTH_PRE_SHIP	,A2	; Pre-ship code		| 753 thru 754
	RTH_BUYER_QL	,A3	; Holds the buyer ql	| 755 thru 757
			,A107	; Filler		| 758 thru 864
				; 			|
RECORD, X
	RTH_KEY		,A45	; Sort/compare key

