RECORD CASCSN_TBL
;
; CASCSN_TBL.DEF - Shipping Container / Serial # Profile.
;
;      Structure:  ISAM
;  Record Length:    50
;    
; ------------------------------------------------------------------------------
;                  ------- Record Definition -------
; ------------------------------------------------------------------------------
	CST_TP_DESIG		,A2	; Trading Partner Designator
	CST_DEST_ABBRV		,A5	; Destination Abbreviation
	CST_OUR_PART		,A15	; Our Part Number
	CST_TARE		,D1	; Include Tare Levels (Y/N) 
	CST_CONT_SERIAL		,D1	; Include Container / Serial Numbers
					; (Y/N)
				,A26

; ------------------------------------------------------------------------------
;               ------- Key Definition ------
; ------------------------------------------------------------------------------

RECORD CST_KEY0	,X	
	K0_CST_TP_DESIG		,A%SIZE(CST_TP_DESIG)
	K0_CST_DEST_ABBRV	,A%SIZE(CST_DEST_ABBRV)
	K0_CST_OUR_PART		,A%SIZE(CST_OUR_PART)
