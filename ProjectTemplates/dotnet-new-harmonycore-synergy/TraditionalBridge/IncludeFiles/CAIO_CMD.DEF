; CAIO_CMD.DEF
;
; CARaS LITERALS - FOR CALLING UIO SUBROUTINE (IO_CMD = One of there...)
;-------------------------------------------------------------------------------
;
RECORD CAIO_CMD.DEF
	; Open Commands
	OPEN_II		,A*,	'OPEN_II'	;Open Indexed Input
	OPEN_UI		,A*,	'OPEN_UI'	;Open Indexed Update
	OPEN_IS		,A*,	'OPEN_IS'	;Open Sequential Input
	OPEN_OS		,A*,	'OPEN_OS'	;Open Sequential Output
	OPEN_IIN	,A*,	'OPEN_II'	;Open Indexed Input No msg
	OPEN_UIN	,A*,	'OPEN_UI'	;Open Indexed Update No Msg
	OPEN_ISN	,A*,	'OPEN_IS'	;Open Sequential Input No Msg
	OPEN_OSN	,A*,	'OPEN_OS'	;Open Sequential Output No Msg
	; Read Commands
	RO		,A2,	'R0'		;Read Indexed Key 0
	R1		,A2,	'R1'		;Read Indexed Key 1
	R2		,A2,	'R2'		;Read Indexed Key 2
	R3		,A2,	'R3'		;Read Indexed Key 3
	R4		,A2,	'R4'		;Read Indexed Key 4
	R5		,A2,	'R5'		;Read Indexed Key 5
	R6		,A2,	'R6'		;Read Indexed Key 6
	RR		,A2,	'RR'		;Read Relative
	RS		,A2,	'RS'		;Read Sequential file
	; Find Commands
	FO		,A2,	'F0'		;Find Indexed Key 0
	F1		,A2,	'F1'		;Find Indexed Key 1
	F2		,A2,	'F2'		;Find Indexed Key 2
	F3		,A2,	'F3'		;Find Indexed Key 3
	F4		,A2,	'F4'		;Find Indexed Key 4
	F5		,A2,	'F5'		;Find Indexed Key 5
	F6		,A2,	'F6'		;Find Indexed Key 6
	; Sequential/Relative Writes
	WS		,A2,	'WS'		;Write Sequential
	WR		,A2,	'WR'		;Write Relative
	; Retrive file path
	PATH		,A5,	'PATH'		; Retrieve Pathname
	; Rfa
	GETRFA		,'G'	'GETRFA'	; Get RFA of the existing record
	;
	; Additional commands available
	; IO_UPDATE	,A1,	'U'	; Update Command
	; IO_DELETE	,A1,	'D'	; Delete Record
	; IO_NEXT	,A1,	'N'	; Delete Record
	; IO_ADD	,A1,	'A'	; Index file add
	; IO_DELETE	,A1,	'D'	; Index file delete
	; IO_NEXT	,A1,	'N'	; Index file read next
	; IO_POSRFA	,A1,	'P'	; Load the RFA and do a read next to
					; re-read the record
