; FORACK_DTL.DEF
;
;	Functional Acknowledgment Detail - Transaction Set Level Information.
;
;	Modified	Programmer	Description
;	--------	----------	-----------
;	09-Jul-89	J. Rheingans	Original
;
;	01-Jun-90	Rich Mae	Added Supplier ID and Destination Code.
;-------------------------------------------------------------------------------
; Organization: Sequential	| Record Size: 100		| Field Position
;-------------------------------------------------------------------------------

RECORD FORACK_DTL
	FAD_SUPPL_ID	,A15	; Supplier ID			|   1 thru 15
	FAD_DEST_CODE	,A15	; Destination Code		|  16 thru 30
	FAD_GROUP_ID	,A2	; Functional Group Identifier	|  31 thru 32
	FAD_DATA_INT	,D9	; Data Interchange Control #	|  33 thru 41
	FAD_TRANS_TYPE	,A3	; Transaction Set Identifier	|  42 thru 44
	FAD_TRANS_CTL	,D9	; Trasnaction Set Control #	|  45 thru 53

	FAD_ACK_CODE	,A1	; Transaction Set Ack Code	|  54 thru 54
	FAD_NOTE_1	,A3	; Transaction Set Note Code-1	|  55 thru 57
	FAD_NOTE_2	,A3	; Transaction Set Note Code-2	|  58 thru 60
	FAD_NOTE_3	,A3	; Transaction Set Note Code-3	|  61 thru 63
	FAD_NOTE_4	,A3	; Transaction Set Note Code-4	|  64 thru 66
	FAD_NOTE_5	,A3	; Transaction Set Note Code-5	|  67 thru 69
			,A31	; Filler			|  70 thru 100

RECORD FAD_KEY, X
	K_FAD_SUPPL_ID		,A%SIZE(FAD_SUPPL_ID)
	K_FAD_DEST_CODE		,A%SIZE(FAD_DEST_CODE)
	K_FAD_GROUP_ID		,A%SIZE(FAD_GROUP_ID)
	K_FAD_DATA_INT		,D%SIZE(FAD_DATA_INT)
	K_FAD_TRANS_TYPE	,A%SIZE(FAD_TRANS_TYPE)
	K_FAD_TRANS_CTL		,D%SIZE(FAD_TRANS_CTL)
