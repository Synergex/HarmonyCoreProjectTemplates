; Literals for I/O routines
	IO_ADD		,A1	,'A'	; Index file add
	IO_DELETE	,A1	,'D'	; Index file delete
	IO_FIND		,A1	,'F'	; Find an Indexed Record.
	IO_GETRFA	,A1	,'G'	; Get the RFA of the existing record
