					; Relative file read by relative
					; record number
	IO_READ_EXACT	,A1	,'E'	; Index file read by specific key
					; for an EXACT match only
	IO_WRITE	,A1	,'W'	; Sequential or relative file write
	IO_UPDATE	,A1	,'U'	; Index file update
	IO_STORE	,A1	,'S'	; 
	IO_ALLOW_ABORT	,D1	,0	; Allow abort on record locked
	IO_NOABORT	,D1	,1	; Do not allow abort on record locked
