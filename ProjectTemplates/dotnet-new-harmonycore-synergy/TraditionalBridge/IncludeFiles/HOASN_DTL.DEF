; HOASN_DTL.DEF
;
;	Honda Maintainable ASN Detail File
;
;-------------------------------------------------------------------------------
;--- CARaS Modification Log ----------------------------------------------------
;-------------------------------------------------------------------------------
;
;	Modified	Programmer	Description
;	---------	----------	------------
;	18-Sept-89	J. Rheingans	Original
;
;	30-Nov-92	T. Bunker	Added KD order number to the key
;	03-Dec-93	T. Bunker	Expand part number to 30.
;					Expand record size from 101 to 128.
;-------------------------------------------------------------------------------
; File Organization: Indexed	| Record Size: 128
;-------------------------------------------------------------------------------

RECORD HOASN_DTL
	HOD_SHIP_ID	,D9	; Shipment (ASN/Shipper) Id Number
	HOD_OUR_PART	,A30	; Our/Supplier Part Number
	HOD_KD_ORDER	,A15	; KD Order Number

	HOD_SHIP_QTY	,D8	; Quantity Shipped
	HOD_UOFM	,A2	; Unit of Measure

	HOD_REL_NO	,A15	; Release Number
	HOD_REQ_DATE	,D6	; Required Date
			,A43	; Filler
RECORD HOD_KEY0, X
	K0_HOD_SHIP_ID		,D%SIZE(HOD_SHIP_ID)
	K0_HOD_OUR_PART		,A%SIZE(HOD_OUR_PART)
	K0_HOD_KD_ORDER		,A%SIZE(HOD_KD_ORDER)

RECORD HOD_KEY1
	K1_HOD_SHIP_ID		,D%SIZE(HOD_SHIP_ID)
	K1_HOD_REL_NO		,A%SIZE(HOD_REL_NO)
	K1_HOD_OUR_PART		,A%SIZE(HOD_OUR_PART)
	K1_HOD_KD_ORDER		,A%SIZE(HOD_KD_ORDER)

